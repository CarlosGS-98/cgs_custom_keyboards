// Latin Alphabet | XKB Keyboard Symbols
// [AUTHOR]: Carlos GonzÃ¡lez Sanz

// [NOTE]: These keyboards are partially based upon the
// default "latin" and "es" symbols in regards to the
// physical key layout.
//
// Put this file on either ~/.config/xkb/symbols
// or in the same directory as
// the other system-wide keyboard layouts.

// Default SMK-Latin Keyboard
default partial alphanumeric_keys
xkb_symbols "basic" {
    // [NOTE]: Keys are arranged the same way as
    // in the standard Spanish QWERTY keyboard.

    // [KEYBOARD LAYOUT REFERENCE (Expanded from SMK v15.x)]:
    // â”Œâ”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”²â”â”â”â”â”â”â”â”â”â”“
    // â”‚ Âª â—Œ â”‚ ! ğ¤€ â”‚ " áµ… â”‚ Â·   â”‚ $   â”‚ %   â”‚ &   â”‚ /   â”‚ ( â†» â”‚ )   â”‚ = â†º â”‚ ? É â”‚ Â¿ ê‹ â”ƒ âŒ«       â”ƒ
    // â”‚ Âº \ â”‚ 1 | â”‚ 2 @ â”‚ 3 # â”‚ 4 â—ŒÌƒ â”‚ 5  á· â”‚ 6 Â¬ â”‚ 7 â—ŒÌ¸ â”‚ 8 âˆ â”‚ 9  á·˜ â”‚ 0  Íš â”‚ ' É‚ â”‚ Â¡ êŒ â”ƒBackspaceâ”ƒ
    // â”¢â”â”â”â”â”â”·â”â”±â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”ºâ”â”³â”â”â”â”â”â”â”â”«
    // â”ƒ       â”ƒ Q   â”‚ W â„² â”‚ E Â¤ â”‚ R É» â”‚ T Ã â”‚ Y êŸš â”‚ U Å® â”‚ I Ä° â”‚ O Ã˜ â”‚ P â†ƒ â”‚ â—ŒÌ‚ â—ŒÌ â”‚ * â—ŒÌ› â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ q   â”‚ w â… â”‚ e â‚¬ â”‚ r É¹ â”‚ t Ã¾ â”‚ y êŸ› â”‚ u Å¯ â”‚ i Ä± â”‚ o Ã¸ â”‚ p â†„ â”‚ â—ŒÌ€ [ â”‚ + ] â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ A Ã… â”‚ S áº â”‚ D Ã â”‚ F   â”‚ G Æ¢ â”‚ H â±µ â”‚ J ÉŸ â”‚ K   â”‚ L Å â”‚ Ã‘ â—ŒÌŒ â”‚ â—ŒÌˆ â—ŒÌ‹ â”‚ Ã‡ â—ŒÌ¨ â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ a Ã¥ â”‚ s ÃŸ â”‚ d Ã° â”‚ f   â”‚ g Æ£ â”‚ h â±¶ â”‚ j È· â”‚ k   â”‚ l Å‚ â”‚ Ã± â—ŒÌ„ â”‚ â—ŒÌ { â”‚ Ã§ } â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ > Ã† â”‚ Z Æ· â”‚ X Æ§ â”‚ C â—ŒÌ‘ â”‚ V â—ŒÌŠ â”‚ B âŒ¬ â”‚ N ÅŠ â”‚ Ğœ  Í› â”‚ ;   â”‚ : â—ŒÌ£ â”‚ _ â—ŒÌ‰ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < Ã¦ â”‚ z Ê’ â”‚ x Æ¨ â”‚ c â—ŒÌ§ â”‚ v â—ŒÌ† â”‚ b â€» â”‚ n Å‹ â”‚ m â—ŒÌ¡Ì¢ â”‚ , â—ŒÌ¦ â”‚ . â—ŒÌ‡ â”‚ - â—ŒÌµ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    name[Group1]= "SMK-Latin (QWERTY)";

    key <TLDE>	{ [  masculine,  ordfeminine,       backslash,              U25CC               ] };    // U+25CC = Dotted Circle
    key <AE01>	{ [          1,  exclam,            bar,                    U10900              ] };    // U+10900 = Phoenician Ê¾Ä€lep ("Phoenician Type" Dead Key)
    key <AE02>	{ [          2,  quotedbl,          at,                     dead_greek          ] };
    key <AE03>	{ [          3,  periodcentered,    numbersign              ] };
    key <AE04>	{ [          4,  dollar,            dead_tilde              ] };
    key <AE05>	{ [          5,  percent,           U1DCF                   ] };                        // U+1DCF = "Letter Fusion" Dead Key
    key <AE06>	{ [          6,  ampersand,         notsign                 ] };
    key <AE07>	{ [          7,  slash,             dead_longsolidusoverlay ] };
    key <AE08>	{ [          8,  parenleft,         U221D,                  U21BB               ] };    // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE09>	{ [          9,  parenright,        U1DFB                   ] };                        // U+1DFB = "Historical Letters" Dead Key
    key <AE10>	{ [          0,  equal,             U035A,                  U21BA               ] };    // U+035A = "IPA Symbols" Dead Key; U+21BA = "Letter Flip" Dead Key
    key <AE11>	{ [ apostrophe,  question,          U0242,                  U0241               ] };    // "'", "?", Lowercase Glottal Stop, Uppercase Glottal Stop 
    key <AE12>	{ [ exclamdown,  questiondown,      UA78C,                  UA78B               ] };    // "Â¡", "Â¿", Lowercase Saltillo, Uppercase Saltillo 

    key <AD01>	{ [          q,  Q                  ] };
    key <AD02>	{ [          w,  W,                 U214E,                  U2132               ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum
    key <AD03>	{ [          e,  E,                 EuroSign,               dead_currency       ] };
    key <AD04>	{ [          r,  R,                 U0279,                  U027B               ] };    // "r", "R", "É¹Ì›", "É»Ì›"
    key <AD05>	{ [          t,  T,                 thorn,                  THORN               ] };
    key <AD06>	{ [          y,  Y,                 UA7DB,                  UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AD07>	{ [          u,  U,                 uring,                  Uring               ] };
    key <AD08>	{ [          i,  I,                 idotless,               Iabovedot           ] };
    key <AD09>	{ [          o,  O,                 oslash,                 Oslash              ] };
    key <AD10>	{ [          p,  P,                 U2184,                  U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AD11>	{ [ dead_grave,  dead_circumflex,   bracketleft,            dead_doublegrave    ] };
    key <AD12>	{ [       plus,  asterisk,          bracketright,           dead_horn           ] };

    key <AC01>	{ [          a,  A,                 aring,                  Aring               ] };
    key <AC02>	{ [          s,  S,                 ssharp,                 U1E9E               ] };    // U+1E9E = Uppercase Eszett
    key <AC03>	{ [          d,  D,                 eth,                    ETH                 ] };
    key <AC04>	{ [          f,  F                  ] };
    key <AC05>	{ [          g,  G,                 U01A3,                  U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AC06>	{ [          h,  H,                 U2C76,                  U2C75               ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AC07>	{ [          j,  J,                 U0237,                  U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AC08>	{ [          k,  K                  ] };
    key <AC09>	{ [          l,  L,                 lstroke,                Lstroke             ] };
    key <AC10>	{ [     ntilde,  Ntilde,            dead_macron,            dead_caron          ] };
    key <AC11>	{ [ dead_acute,  dead_diaeresis,    braceleft,              dead_doubleacute    ] };
    key <BKSL>	{ [   ccedilla,  Ccedilla,          braceright,             dead_ogonek         ] };

    key <LSGT>	{ [       less,  greater,           ae,                     AE                  ] };
    key <AB01>	{ [          z,  Z,                 ezh,                    EZH                 ] };
    key <AB02>	{ [          x,  X,                 U01A8,                  U01A7               ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AB03>	{ [          c,  C,                 dead_cedilla,           dead_invertedbreve  ] };
    key <AB04>	{ [          v,  V,                 dead_breve,             dead_abovering      ] };
    key <AB05>	{ [          b,  B,                 U203B,                  U232C               ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
    key <AB06>	{ [          n,  N,                 eng,                    ENG                 ] };
    key <AB07>	{ [          m,  M,                 U0359,                  U035B               ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AB08>	{ [      comma,  semicolon,         dead_belowcomma         ] };
    key <AB09>	{ [     period,  colon,             dead_abovedot,          dead_belowdot       ] };
    key <AB10>	{ [      minus,  underscore,        dead_stroke,            dead_hook           ] };

    key <SPCE>  { [      space,  space,             nobreakspace            ] };

    // Default modifiers
    include "level3(ralt_switch)"
    include "compose(rctrl-altgr)"  // As a middle ground (feel free to change this line if you want or need to)
};

partial alphanumeric_keys
xkb_symbols "qwerty" {
    include "smk/latin(basic)"
};

partial alphanumeric_keys
xkb_symbols "qwerty_pt" {
    // [NOTE]: This layout is based off a mixture between
    // the Portuguese QWERTY keyboard
    // and the Spanish QWERTY one with some
    // custom rearrangements.

    // [KEYBOARD LAYOUT REFERENCE]:
    // â”Œâ”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”²â”â”â”â”â”â”â”â”â”â”“
    // â”‚ | ê‹ â”‚ !   â”‚ " áµ… â”‚ # Â· â”‚ $   â”‚ %   â”‚ &   â”‚ / â—ŒÌ¸ â”‚ ( âˆ â”‚ ) â†» â”‚ = â†º â”‚ ? É â”‚ Â» Â¿ â”ƒ âŒ«       â”ƒ
    // â”‚ \ êŒ â”‚ 1 ğ¤Ÿ â”‚ 2 @ â”‚ 3 Â£ â”‚ 4 Â§ â”‚ 5  á· â”‚ 6 Â¬ â”‚ 7 { â”‚ 8 [ â”‚ 9 ] â”‚ 0 } â”‚ ' É‚ â”‚ Â« Â¡ â”ƒBackspaceâ”ƒ
    // â”¢â”â”â”â”â”â”·â”â”±â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”ºâ”â”³â”â”â”â”â”â”â”â”«
    // â”ƒ       â”ƒ Q   â”‚ W â„² â”‚ E Â¤ â”‚ R É» â”‚ T Ã â”‚ Y êŸš â”‚ U Å® â”‚ I Ä° â”‚ O Ã˜ â”‚ P â†ƒ â”‚ * â—Œ â”‚ â—ŒÌ€ â—ŒÌ â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ q   â”‚ w â… â”‚ e â‚¬ â”‚ r É¹ â”‚ t Ã¾ â”‚ y êŸ› â”‚ u Å¯ â”‚ i Ä± â”‚ o Ã¸ â”‚ p â†„ â”‚ + â—ŒÌˆ â”‚ â—ŒÌ â—ŒÌ‹ â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ A Ã… â”‚ S áº â”‚ D Ã â”‚ F   â”‚ G Æ¢ â”‚ H â±µ â”‚ J ÉŸ â”‚ K   â”‚ L Å â”‚ Ã‡ â—ŒÌŒ â”‚ Âª  Íš â”‚ â—ŒÌ‚ â—ŒÌ¨ â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ a Ã¥ â”‚ s ÃŸ â”‚ d Ã° â”‚ f   â”‚ g Æ£ â”‚ h â±¶ â”‚ j È· â”‚ k   â”‚ l Å‚ â”‚ Ã§ â—ŒÌ„ â”‚ Âº  á·˜ â”‚ â—ŒÌƒ â—ŒÌ› â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ > Ã† â”‚ Z Æ· â”‚ X Æ§ â”‚ C â—ŒÌ‘ â”‚ V â—ŒÌŠ â”‚ B âŒ¬ â”‚ N ÅŠ â”‚ Ğœ  Í› â”‚ ; â—ŒÌ“ â”‚ : â—ŒÌ£ â”‚ _ â—ŒÌ‰ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < Ã¦ â”‚ z Ê’ â”‚ x Æ¨ â”‚ c â—ŒÌ§ â”‚ v â—ŒÌ† â”‚ b â€» â”‚ n Å‹ â”‚ m â—ŒÌ¡Ì¢ â”‚ , â—ŒÌ¦ â”‚ . â—ŒÌ‡ â”‚ - â—ŒÌµ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    key <TLDE>	{ [     backslash,  bar,               UA78C,                  UA78B                   ] };     // "\", "|", Lowercase Saltillo, Uppercase Saltillo 
    key <AE01>	{ [             1,  exclam,            U10900                  ] };                             // U+10900 = Phoenician Ê¾Ä€lep ("Phoenician Type" Dead Key)
    key <AE03>	{ [             3,  numbersign,        sterling,               periodcentered          ] };
    key <AE04>	{ [             4,  dollar,            section                 ] };
    key <AE07>	{ [             7,  slash,             braceleft,              dead_longsolidusoverlay ] };
    key <AE08>	{ [             8,  parenleft,         bracketleft,            U221D                   ] };     // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE09>	{ [             9,  parenright,        bracketright,           U21BB                   ] };     // U+21BB = "Arrows" Dead Key
    key <AE10>	{ [             0,  equal,             braceright,             U21BA                   ] };     // U+21BA = "Letter Flip" Dead Key
    key <AE12>	{ [ guillemetleft,  guillemetright,    exclamdown,             questiondown            ] };

    key <AD11>	{ [          plus,  asterisk,          dead_diaeresis,         U25CC                   ] };     // U+25CC = Dotted Circle
    key <AD12>	{ [    dead_acute,  dead_grave,        dead_doubleacute,       dead_doublegrave        ] };

    key <AC10>	{ [      ccedilla,  Ccedilla,          dead_macron,            dead_caron              ] };
    key <AC11>	{ [     masculine,  ordfeminine,       U1DCF,                  U035A                   ] };     // U+1DCF = "Letter Fusion" Dead Key; U+035A = "IPA Symbols" Dead Key 
    key <BKSL>	{ [    dead_tilde,  dead_circumflex,   dead_horn,              dead_ogonek             ] };
};

partial alphanumeric_keys
xkb_symbols "qwerty_us" {
    // [NOTE]: This layout is based off a mixture between
    // the American QWERTY keyboard
    // and the Spanish QWERTY one with some
    // custom rearrangements.

    // [KEYBOARD LAYOUT REFERENCE]:
    // â”Œâ”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”²â”â”â”â”â”â”â”â”â”â”“
    // â”‚ â—ŒÌƒ â—Œ â”‚ !   â”‚ @   â”‚ #   â”‚ $   â”‚ %   â”‚ â—ŒÌ‚   â”‚ &   â”‚ * â†» â”‚ (   â”‚ ) â†º â”‚ _ â—ŒÌ„ â”‚ + Â¿ â”ƒ âŒ«       â”ƒ
    // â”‚ â—ŒÌ€ â—ŒÌ â”‚ 1 ğ¤Ÿ â”‚ 2 áµ… â”‚ 3 Â· â”‚ 4 Â¤ â”‚ 5  á· â”‚ 6 Â¬ â”‚ 7   â”‚ 8 âˆ â”‚ 9  á·˜ â”‚ 0  Íš â”‚ - â—ŒÌµ â”‚ = Â¡ â”ƒBackspaceâ”ƒ
    // â”¢â”â”â”â”â”â”·â”â”±â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”ºâ”â”³â”â”â”â”â”â”â”â”«
    // â”ƒ       â”ƒ Q   â”‚ W â„² â”‚ E Æ â”‚ R É» â”‚ T Ã â”‚ Y êŸš â”‚ U Å® â”‚ I Ä° â”‚ O Ã˜ â”‚ P â†ƒ â”‚ { É â”‚ } ê‹ â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ q   â”‚ w â… â”‚ e Ç â”‚ r É¹ â”‚ t Ã¾ â”‚ y êŸ› â”‚ u Å¯ â”‚ i Ä± â”‚ o Ã¸ â”‚ p â†„ â”‚ [ É‚ â”‚ ] êŒ â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ A Ã… â”‚ S áº â”‚ D Ã â”‚ F   â”‚ G Æ¢ â”‚ H â±µ â”‚ J ÉŸ â”‚ K   â”‚ L Å â”‚ : â—ŒÌŒ â”‚ " â—ŒÌ‹ â”‚ | â—ŒÌ› â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ a Ã¥ â”‚ s ÃŸ â”‚ d Ã° â”‚ f   â”‚ g Æ£ â”‚ h â±¶ â”‚ j È· â”‚ k   â”‚ l Å‚ â”‚ ; â—ŒÌˆ â”‚ ' â—ŒÌ â”‚ \ â—ŒÌ¨ â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ | Ã† â”‚ Z Æ· â”‚ X Æ§ â”‚ C â—ŒÌ‘ â”‚ V â—ŒÌŠ â”‚ B âŒ¬ â”‚ N ÅŠ â”‚ Ğœ  Í› â”‚ <   â”‚ > â—ŒÌ£ â”‚ ? â—ŒÌ‰ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ \ Ã¦ â”‚ z Ê’ â”‚ x Æ¨ â”‚ c â—ŒÌ§ â”‚ v â—ŒÌ† â”‚ b â€» â”‚ n Å‹ â”‚ m â—ŒÌ¡Ì¢ â”‚ , â—ŒÌ¦ â”‚ . â—ŒÌ‡ â”‚ / â—ŒÌ¸ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (US QWERTY)";

    key <TLDE>	{ [   dead_grave,  dead_tilde,        dead_doublegrave,         U25CC               ] };    // U+25CC = Dotted Circle
    key <AE01>	{ [            1,  exclam,            U10900                    ] };                        // U+10900 = Phoenician Ê¾Ä€lep ("Phoenician Type" Dead Key)
    key <AE02>	{ [            2,  at,                dead_greek                ] };
    key <AE03>	{ [            3,  numbersign,        periodcentered            ] };
    key <AE04>	{ [            4,  dollar,            dead_currency             ] };
    key <AE05>	{ [            5,  percent,           U1DCF                     ] };                        // U+1DCF = "Letter Fusion" Dead Key
    key <AE06>	{ [            6,  dead_circumflex,   notsign                   ] };
    key <AE07>	{ [            7,  ampersand          ] };
    key <AE08>	{ [            8,  asterisk,          U221D,                    U21BB               ] };    // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE09>	{ [            9,  parenleft,         U1DFB                     ] };                        // U+1DFB = "Historical Letters" Dead Key
    key <AE10>	{ [            0,  parenright,        U035A,                    U21BA               ] };    // U+035A = "IPA Symbols" Dead Key; U+21BA = "Letter Flip" Dead Key
    key <AE11>	{ [        minus,  underscore,        dead_stroke,              dead_macron         ] };
    key <AE12>	{ [        equal,  plus,              exclamdown,               questiondown        ] };

    key <AD03>	{ [            e,  E,                 schwa,                    SCHWA               ] };
    key <AD11>	{ [  bracketleft,  braceleft,         U0242,                    U0241               ] };    // "[", "{", Lowercase Glottal Stop, Uppercase Glottal Stop 
    key <AD12>	{ [ bracketright,  braceright,        UA78C,                    UA78B               ] };    // "]", "}", Lowercase Saltillo, Uppercase Saltillo

    key <AC10>	{ [    semicolon,  colon,             dead_diaeresis,           dead_caron          ] };
    key <AC11>	{ [   apostrophe,  quotedbl,          dead_acute,               dead_doubleacute    ] };
    key <BKSL>	{ [    backslash,  bar,               dead_ogonek,              dead_horn           ] };

    key <LSGT>	{ [    backslash,  bar,               ae,                       AE                  ] };
    key <AB08>	{ [        comma,  less,              dead_belowcomma           ] };
    key <AB09>	{ [       period,  greater,           dead_abovedot,            dead_belowdot       ] };
    key <AB10>	{ [       minus,   underscore,        dead_longsolidusoverlay,  dead_hook           ] };
};

partial alphanumeric_keys
xkb_symbols "qwertz" {
    // [NOTE]: This layout is based off a mixture between
    // the Austrian-German QWERTZ keyboard
    // and the Spanish QWERTY one with some
    // custom rearrangements.

    // [KEYBOARD LAYOUT REFERENCE]:
