    // â”ƒ      â”ƒ > Â¦ â”‚ J ÉŸ â”‚ Ã– Å’ â”‚ V â—ŒÌ‘ â”‚ C Ã‡ â”‚ â—ŒÌƒ â—ŒÌ› â”‚ Z   â”‚ S Å â”‚ B âŒ¬ â”‚ : â—ŒÌ£ â”‚ ; âˆ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < | â”‚ j È· â”‚ Ã¶ Å“ â”‚ v â—ŒÌ† â”‚ c Ã§ â”‚ â—ŒÌ§ â—ŒÌ¨ â”‚ z   â”‚ s ÅŸ â”‚ b â€» â”‚ . â—ŒÌ‡ â”‚ , â—ŒÌ¦ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (FGÄIOD)";

    key <TLDE>	{ [         plus,   asterisk,           notsign,              U25CC                     ] };    // U+25CC = Dotted Circle
    key <AE01>	{ [            1,   exclam,             onesuperior,          exclamdown                ] };
    key <AE02>	{ [            2,   quotedbl,           twosuperior,          dead_greek                ] };
    key <AE03>	{ [            3,   dead_circumflex,    numbersign,           threesuperior             ] };
    key <AE04>	{ [            4,   dollar,             LiraSign,             dead_currency             ] };
    key <AE05>	{ [            5,   percent,            U1DCF,                sterling                  ] };    // U+1DCF = "Letter Fusion" Dead Key
    key <AE06>	{ [            6,   ampersand,          bracketleft,          ordfeminine               ] };
    key <AE07>	{ [            7,   apostrophe,         braceleft,            dead_longsolidusoverlay   ] };
    key <AE08>	{ [            8,   parenleft,          braceright,           U21BB                     ] };    // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE11>	{ [        slash,   question,           backslash,            questiondown              ] };
    key <AE12>	{ [        minus,   underscore,         dead_stroke,          dead_hook                 ] };

    key <AD01>	{ [            f,   F,                  at,                   masculine                 ] };
    key <AD02>	{ [            g,   G,                  U01A3,                U01A2                     ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AD03>	{ [       gbreve,   Gbreve,             U0242,                U0241                     ] };    // "ÄŸ", "Ä", Lowercase Glottal Stop, Uppercase Glottal Stop 
    key <AD04>	{ [     idotless,   I,                  i,                    Iabovedot                 ] };
    key <AD05>	{ [            o,   O,                  oslash,               Oslash                    ] };
    key <AD06>	{ [            d,   D,                  eth,                  ETH                       ] };
    key <AD07>	{ [            r,   R,                  U0279,                U027B                     ] };    // "r", "R", "É¹Ì›", "É»Ì›"
    key <AD08>	{ [            n,   N,                  eng,                  ENG                       ] };
    key <AD09>	{ [            h,   H,                  U2C76,                U2C75                     ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AD11>	{ [            q,   Q,                  dead_diaeresis,       dead_abovering            ] };
    key <AD12>	{ [            w,   W,                  U214E,                U2132                     ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum

    key <AC01>	{ [            u,   U,                  uring,                Uring                     ] };
    key <AC02>	{ [       ssharp,   U1E9E,              ezh,                  EZH                       ] };    // U+1E9E = Uppercase Eszett
    key <AC03>	{ [            e,   E,                  schwa,                SCHWA                     ] };
    key <AC04>	{ [            a,   A,                  ae,                   AE                        ] };
    key <AC05>	{ [   udiaeresis,   Udiaeresis,         UA78C,                UA78B                     ] };    // "Ã¼", "Ãœ", Lowercase Saltillo, Uppercase Saltillo 
    key <AC06>	{ [            t,   T,                  thorn,                THORN                     ] };
    key <AC07>	{ [            k,   K,                  dead_macron,          dead_caron                ] };
    key <AC08>	{ [            m,   M,                  U0359,                U035B                     ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AC10>	{ [            y,   Y,                  UA7DB,                UA7DA                     ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AC11>	{ [   dead_acute,   dead_grave,         dead_doubleacute,     dead_doublegrave          ] };
    key <BKSL>	{ [            x,   X,                  U01A8,                U01A7                     ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two

    key <LSGT>	{ [         less,   greater,            bar,                  brokenbar                 ] };
    key <AB01>	{ [            j,   J,                  U0237,                U025F                     ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AB02>	{ [   odiaeresis,   Odiaeresis,         oe,                   OE                        ] };
    key <AB03>	{ [            v,   V,                  dead_breve,           dead_invertedbreve        ] };
    key <AB04>	{ [            c,   C,                  ccedilla,             Ccedilla                  ] };
    key <AB05>	{ [ dead_cedilla,   dead_tilde,         dead_ogonek,          dead_horn                 ] };
    key <AB06>	{ [            z,   Z                   ] };
    key <AB07>	{ [            s,   S,                  scedilla,             Scedilla                  ] };
    key <AB08>	{ [            b,   B,                  U203B,                U232C                     ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
    key <AB10>	{ [        comma,   semicolon,          dead_belowcomma,      U221D                     ] };    // U+221D = "Logic + Math Symbols" Dead Key
