xkb_symbols "jcuken" {
    // [NOTE]: This layout is based off a mixture between
    // the Russian Ğ™Ğ¦Ğ£ĞšĞ•Ğ keyboard (its approximate Latin-based form)
    // and the Spanish QWERTY one with some
    // custom rearrangements.

    // [KEYBOARD LAYOUT REFERENCE (Expanded from SMK v15.x)]:
    // â”Œâ”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”²â”â”â”â”â”â”â”â”â”â”“
    // â”‚ Âª â—Œ â”‚ ! ğ¤€ â”‚ " áµ… â”‚ Â·   â”‚ $   â”‚ %   â”‚ &   â”‚ /   â”‚ ( â†» â”‚ )   â”‚ = â†º â”‚ ?   â”‚ Â¿   â”ƒ âŒ«       â”ƒ
    // â”‚ Âº \ â”‚ 1 | â”‚ 2 @ â”‚ 3 # â”‚ 4 â—ŒÌƒ â”‚ 5  á· â”‚ 6 Â¬ â”‚ 7 â—ŒÌ¸ â”‚ 8 âˆ â”‚ 9  á·˜ â”‚ 0  Íš â”‚ '   â”‚ Â¡   â”ƒBackspaceâ”ƒ
    // â”¢â”â”â”â”â”â”·â”â”±â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”ºâ”â”³â”â”â”â”â”â”â”â”«
    // â”ƒ       â”ƒ J ÉŸ â”‚ C ÄŒ â”‚ U Å® â”‚ K   â”‚ E Â¤ â”‚ N ÅŠ â”‚ G Æ¢ â”‚ Z Å½ â”‚ X Æ§ â”‚ É ê‹ â”‚ â—ŒÌ‚ â—ŒÌ â”‚ * â—ŒÌ› â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ j È· â”‚ c Ä â”‚ u Å¯ â”‚ k   â”‚ e â‚¬ â”‚ n Å‹ â”‚ g Æ£ â”‚ z Å¾ â”‚ x Æ¨ â”‚ É‚ êŒ â”‚ â—ŒÌ€ [ â”‚ + ] â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ F   â”‚ I Ä° â”‚ V â—ŒÌŠ â”‚ A Ã… â”‚ P â†ƒ â”‚ R É» â”‚ O Ã˜ â”‚ L Å â”‚ áº Æ· â”‚ Q â—ŒÌŒ â”‚ â—ŒÌˆ â—ŒÌ‹ â”‚ Ã‘ â—ŒÌ¨ â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ f   â”‚ i Ä± â”‚ v â—ŒÌ† â”‚ a Ã¥ â”‚ p â†„ â”‚ r É¹ â”‚ o Ã¸ â”‚ l Å‚ â”‚ ÃŸ Ê’ â”‚ q â—ŒÌ„ â”‚ â—ŒÌ { â”‚ Ã± } â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ > Ã† â”‚ Y êŸš â”‚ Ã‡ â—ŒÌ‘ â”‚ S Å  â”‚ Ğœ  Í› â”‚ ê® Æ– â”‚ T Ã â”‚ B âŒ¬ â”‚ ;   â”‚ : â—ŒÌ£ â”‚ _ â—ŒÌ‰ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < Ã¦ â”‚ y êŸ› â”‚ Ã§ â—ŒÌ§ â”‚ s Å¡ â”‚ m â—ŒÌ¡Ì¢ â”‚ Éª É© â”‚ t Ã¾ â”‚ b â€» â”‚ , â—ŒÌ¦ â”‚ . â—ŒÌ‡ â”‚ - â—ŒÌµ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (JCUKEN)";

    key <AE11>	{ [ apostrophe,  question           ] };
    key <AE12>	{ [ exclamdown,  questiondown       ] };

    key <AD01>	{ [          j,  J,                 U0237,                  U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AD02>	{ [          c,  C,                 ccaron,                 Ccaron              ] };
    key <AD03>	{ [          u,  U,                 uring,                  Uring               ] };
    key <AD04>	{ [          k,  K                  ] };
    key <AD05>	{ [          e,  E,                 EuroSign,               dead_currency       ] };
    key <AD06>	{ [          n,  N,                 eng,                    ENG                 ] };
    key <AD07>	{ [          g,  G,                 U01A3,                  U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AD08>	{ [          z,  Z,                 zcaron,                 Zcaron              ] };
    key <AD09>	{ [          x,  X,                 U01A8,                  U01A7               ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AD10>	{ [     U0242,   U0241,             UA78C,                  UA78B               ] };    // Lowercase Glottal Stop, Uppercase Glottal Stop, Lowercase Saltillo, Uppercase Saltillo 

    key <AC01>	{ [          f,  F                  ] };
    key <AC02>	{ [          i,  I,                 idotless,               Iabovedot           ] };
    key <AC03>	{ [          v,  V,                 dead_breve,             dead_abovering      ] };
    key <AC04>	{ [          a,  A,                 aring,                  Aring               ] };
    key <AC05>	{ [          p,  P,                 U2184,                  U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AC06>	{ [          r,  R,                 U0279,                  U027B               ] };    // "r", "R", "É¹Ì›", "É»Ì›"
    key <AC07>	{ [          o,  O,                 oslash,                 Oslash              ] };
    key <AC08>	{ [          l,  L,                 lstroke,                Lstroke             ] };
    key <AC09>	{ [     ssharp,  U1E9E,             ezh,                    EZH                 ] };    // U+1E9E = Uppercase Eszett
    key <AC10>	{ [          q,  Q,                 dead_macron,            dead_caron          ] };
    key <BKSL>	{ [     ntilde,  Ntilde,            braceright,             dead_ogonek         ] };

    key <AB01>	{ [          y,  Y,                 UA7DB,                  UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AB02>	{ [   ccedilla,  Ccedilla,          dead_cedilla,           dead_invertedbreve  ] };
    key <AB03>	{ [          s,  S,                 scaron,                 Scaron              ] };    // U+1E9E = Uppercase Eszett
    key <AB04>	{ [          m,  M,                 U0359,                  U035B               ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AB05>	{ [      U026A,  UA7AE,             U0269,                  U0196               ] };    // Lowercase Smallcaps I, Uppercase Smallcaps I, Lowercase Latin Iota, Uppercase Latin Iota
    key <AB06>	{ [          t,  T,                 thorn,                  THORN               ] };
    key <AB07>	{ [          b,  B,                 U203B,                  U232C               ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
};

