    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(qwerty_pt)"

    name[Group1]= "SMK-Latin (HCESAR)";

    key <TLDE>	{ [  masculine,  ordfeminine,       U1DCF,                  U035A               ] };     // U+1DCF = "Letter Fusion" Dead Key; U+035A = "IPA Symbols" Dead Key

    key <AD01>	{ [      minus,  underscore,        dead_stroke,            dead_hook           ] };
    key <AD02>	{ [          h,  H,                 U2C76,                  U2C75               ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AD03>	{ [          c,  C,                 dead_cedilla,           dead_invertedbreve  ] };
    key <AD04>	{ [          e,  E,                 EuroSign,               dead_currency       ] };
    key <AD05>	{ [          s,  S,                 ssharp,                 U1E9E               ] };    // U+1E9E = Uppercase Eszett
    key <AD06>	{ [          a,  A,                 aring,                  Aring               ] };
    key <AD07>	{ [          r,  R,                 U0279,                  U027B               ] };    // "r", "R", "É¹Ì›", "É»Ì›"
    key <AD08>	{ [          o,  O,                 oslash,                 Oslash              ] };
    key <AD09>	{ [          p,  P,                 U2184,                  U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AD10>	{ [          z,  Z,                 ezh,                    EZH                 ] };

    key <AC01>	{ [  backslash,  bar,               UA78C,                  UA78B               ] };    // "\", "|", Lowercase Saltillo, Uppercase Saltillo 
    key <AC02>	{ [          q,  Q                  ] };
    key <AC03>	{ [          t,  T,                 thorn,                  THORN               ] };
    key <AC04>	{ [          d,  D,                 eth,                    ETH                 ] };
    key <AC05>	{ [          i,  I,                 idotless,               Iabovedot           ] };
    key <AC06>	{ [          n,  N,                 eng,                    ENG                 ] };
    key <AC07>	{ [          u,  U,                 uring,                  Uring               ] };
    key <AC08>	{ [          l,  L,                 lstroke,                Lstroke             ] };
    key <AC09>	{ [          m,  M,                 U0359,                  U035B               ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AC10>	{ [          x,  X,                 U01A8,                  U01A7               ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AC11>	{ [      comma,  semicolon,         dead_belowcomma         ] };

    key <AB01>	{ [          y,  Y,                 UA7DB,                  UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AB02>	{ [   ccedilla,  Ccedilla,          dead_macron,            dead_caron          ] };
    key <AB03>	{ [          j,  J,                 U0237,                  U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AB04>	{ [          b,  B,                 U203B,                  U232C               ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
    key <AB05>	{ [          f,  F                  ] };
    key <AB06>	{ [          v,  V,                 dead_breve,             dead_abovering      ] };
    key <AB07>	{ [          g,  G,                 U01A3,                  U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AB08>	{ [          k,  K                  ] };
    key <AB09>	{ [          w,  W,                 U214E,                  U2132               ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum
    key <AB10>	{ [     period,  colon,             dead_abovedot,          dead_belowdot       ] };
};

