    // â”ƒ       â”ƒ A Ã… â”‚ Z Æ· â”‚ E Â¤ â”‚ R É» â”‚ T Ã â”‚ Y êŸš â”‚ U Å® â”‚ I Ä° â”‚ O Ã˜ â”‚ P â†ƒ â”‚ â—ŒÌ¨   â”‚ * â—ŒÌ› â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ a Ã¥ â”‚ z Ê’ â”‚ e â‚¬ â”‚ r É¹ â”‚ t Ã¾ â”‚ y êŸ› â”‚ u Å¯ â”‚ i Ä± â”‚ o Ã¸ â”‚ p â†„ â”‚ â—ŒÌˆ [ â”‚ $ ] â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ Q   â”‚ S áº â”‚ D Ã â”‚ F   â”‚ G Æ¢ â”‚ H â±µ â”‚ J ÉŸ â”‚ K   â”‚ L Å â”‚ M â—ŒÌŒ â”‚ %   â”‚ Â£ â—ŒÌ â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ q   â”‚ s ÃŸ â”‚ d Ã° â”‚ f   â”‚ g Æ£ â”‚ h â±¶ â”‚ j È· â”‚ k   â”‚ l Å‚ â”‚ m â—ŒÌ„ â”‚ â—ŒÌ â—ŒÌ‹ â”‚ Âµ â—ŒÌ€ â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ > Ã† â”‚ W â„² â”‚ X Æ§ â”‚ C â—ŒÌ‘ â”‚ V â—ŒÌŠ â”‚ B âŒ¬ â”‚ N ÅŠ â”‚ ?  Í› â”‚ .   â”‚ / â—ŒÌ£ â”‚ + â—ŒÌ‰ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < Ã¦ â”‚ w â… â”‚ x Æ¨ â”‚ c â—ŒÌ§ â”‚ v â—ŒÌ† â”‚ b â€» â”‚ n Å‹ â”‚ , â—ŒÌ¡Ì¢ â”‚ ; â—ŒÌ¦ â”‚ : â—ŒÌ‡ â”‚ = â—ŒÌµ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (AZERTY)";

    key <TLDE>	{ [    twosuperior,  threesuperior,    backslash,               U25CC               ] };    // U+25CC = Dotted Circle
    key <AE01>	{ [      ampersand,  1,                bar,                     U10900              ] };    // U+10900 = Phoenician Ê¾Ä€lep ("Phoenician Type" Dead Key)
    key <AE02>	{ [     dead_greek,  2,                at                       ] };
    key <AE03>	{ [       quotedbl,  3,                numbersign,              periodcentered      ] };
    key <AE04>	{ [     apostrophe,  4,                dead_tilde               ] };
    key <AE05>	{ [      parenleft,  5,                U1DCF                    ] };                        // U+1DCF = "Letter Fusion" Dead Key
    key <AE06>	{ [        section,  6,                dead_circumflex          ] };
    key <AE07>	{ [        notsign,  7,                dead_longsolidusoverlay  ] };
    key <AE08>	{ [         exclam,  8,                U221D,                   U21BB               ] };    // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE09>	{ [          U1DFB,  9,                braceleft                ] };                        // U+1DFB = "Historical Letters" Dead Key
    key <AE10>	{ [          U035A,  0,                braceright,              U21BA               ] };    // U+035A = "IPA Symbols" Dead Key; U+21BA = "Letter Flip" Dead Key
    key <AE11>	{ [     parenright,  masculine,        U0242,                   U0241               ] };    // ")", "Âº", Lowercase Glottal Stop, Uppercase Glottal Stop 
    key <AE12>	{ [          minus,  underscore,       UA78C,                   UA78B               ] };    // "-", "_", Lowercase Saltillo, Uppercase Saltillo 

    key <AD01>	{ [              a,  A,                aring,                   Aring               ] };
    key <AD02>	{ [              z,  Z,                ezh,                     EZH                 ] };
    key <AD11>	{ [ dead_diaeresis,  dead_ogonek,      bracketleft              ] };
    key <AD12>	{ [         dollar,  asterisk,         bracketright,            dead_horn           ] };

    key <AC01>	{ [              q,  Q                 ] };
    key <AC10>	{ [              m,  M,                dead_macron,             dead_caron          ] };
    key <AC11>	{ [     dead_acute,  percent,          dead_doubleacute         ] };
    key <BKSL>	{ [             mu,  sterling,         dead_grave,              dead_doublegrave    ] };

    key <AB01>	{ [             w,   W,                U214E,                   U2132               ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum
    key <AB07>	{ [         comma,   question,         U0359,                   U035B               ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AB08>	{ [     semicolon,   period,           dead_belowcomma          ] };
    key <AB09>	{ [         colon,   slash,            dead_abovedot,           dead_belowdot       ] };
    key <AB10>	{ [         equal,   plus,             dead_stroke,             dead_hook           ] };
};

partial alphanumeric_keys
xkb_symbols "lithuanian_azerty" {
    // [NOTE]: This layout is based off a mixture between
    // the Lithuanian Ä„Å½ERTY keyboard
    // and the Spanish QWERTY one with some
    // custom rearrangements.

    // [KEYBOARD LAYOUT REFERENCE]:
    // â”Œâ”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”²â”â”â”â”â”â”â”â”â”â”“
    // â”‚ â—ŒÌƒ â—Œ â”‚ 1 ğ¤Ÿ â”‚ 2 áµ… â”‚ 3 â—ŒÌ¸ â”‚ 4 Â· â”‚ 5 Â§ â”‚ 6 Â¬ â”‚ 7  á·˜ â”‚ 8 â†» â”‚ 9  Íš â”‚ 0 â†º â”‚ + " â”‚ X   â”ƒ âŒ«       â”ƒ
    // â”‚ â—ŒÌ€ â—ŒÌ â”‚ ! @ â”‚ - _ â”‚ / # â”‚ ; $ â”‚ :  á· â”‚ , â—ŒÌ‚ â”‚ . & â”‚ = âˆ â”‚ ( [ â”‚ ) ] â”‚ ? ' â”‚ x % â”ƒBackspaceâ”ƒ
    // â”¢â”â”â”â”â”â”·â”â”±â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”ºâ”â”³â”â”â”â”â”â”â”â”«
    // â”ƒ       â”ƒ Ä„ Ã† â”‚ Å½ Æ§ â”‚ E Â¤ â”‚ R É» â”‚ T Ã â”‚ Y êŸš â”‚ U Å² â”‚ I Ä® â”‚ O Ã˜ â”‚ P â†ƒ â”‚ â—ŒÌ¨   â”‚ W â„² â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ Ä… Ã¦ â”‚ Å¾ Æ¨ â”‚ e â‚¬ â”‚ r É¹ â”‚ t Ã¾ â”‚ y êŸ› â”‚ u Å³ â”‚ i Ä¯ â”‚ o Ã¸ â”‚ p â†„ â”‚ â—ŒÌˆ â—ŒÌ› â”‚ w â… â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ A Ã… â”‚ S áº â”‚ D Ã â”‚ Å  É â”‚ G Æ¢ â”‚ H â±µ â”‚ J ÉŸ â”‚ K   â”‚ L Å â”‚ Ä˜ â—ŒÌŒ â”‚ Ä– â—ŒÌ â”‚ Q   â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ a Ã¥ â”‚ s ÃŸ â”‚ d Ã° â”‚ Å¡ É‚ â”‚ g Æ£ â”‚ h â±¶ â”‚ j È· â”‚ k   â”‚ l Å‚ â”‚ Ä™ â—ŒÌ„ â”‚ Ä— â—ŒÌ‹ â”‚ q | â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ > â—ŒÌµ â”‚ Z Æ· â”‚ Åª ê‹ â”‚ C â—ŒÌ‘ â”‚ V â—ŒÌŠ â”‚ B âŒ¬ â”‚ N ÅŠ â”‚ Ğœ  Í› â”‚ ÄŒ   â”‚ F â—ŒÌ£ â”‚ \ â—ŒÌ‰ â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < - â”‚ z Ê’ â”‚ Å« êŒ â”‚ c â—ŒÌ§ â”‚ v â—ŒÌ† â”‚ b â€» â”‚ n Å‹ â”‚ m â—ŒÌ¡Ì¢ â”‚ Ä â—ŒÌ¦ â”‚ f â—ŒÌ‡ â”‚ { } â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (Ä„Å½ERTY)";

    key <TLDE>	{ [     dead_grave,  dead_tilde,    dead_acute,         U25CC                   ] };    // U+25CC = Dotted Circle
    key <AE01>	{ [         exclam,  1,             at                  ] };    // U+10900 = Phoenician Ê¾Ä€lep ("Phoenician Type" Dead Key)
    key <AE02>	{ [          minus,  2,             underscore,         dead_greek              ] };
    key <AE03>	{ [          slash,  3,             numbersign,         dead_longsolidusoverlay ] };
    key <AE04>	{ [      semicolon,  4,             dollar,             periodcentered          ] };
    key <AE05>	{ [          colon,  5,             U1DCF,              section                 ] };    // U+1DCF = "Letter Fusion" Dead Key
    key <AE06>	{ [          comma,  6,             dead_circumflex,    notsign                 ] };
    key <AE07>	{ [         period,  7,             ampersand,          U1DFB                   ] };    // U+1DFB = "Historical Letters" Dead Key
    key <AE08>	{ [          equal,  8,             U221D,              U21BB                   ] };    // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE09>	{ [      parenleft,  9,             bracketleft,        U035A                   ] };    // U+035A = "IPA Symbols" Dead Key
    key <AE10>	{ [     parenright,  0,             bracketright,       U21BA                   ] };    // U+21BA = "Letter Flip" Dead Key
    key <AE11>	{ [       question,  plus,          apostrophe,         quotedbl                ] };
    key <AE12>	{ [              x,  X,             percent             ] };

    key <AD01>	{ [        aogonek,  Aogonek,       ae,                 AE                      ] };
    key <AD02>	{ [         zcaron,  Zcaron,        U01A8,              U01A7                   ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AD07>	{ [              u,  U,             uogonek,            Uogonek                 ] };
    key <AD08>	{ [              i,  I,             iogonek,            Iogonek                 ] };
    key <AD11>	{ [ dead_diaeresis,  dead_ogonek,   dead_horn           ] };
    key <AD12>	{ [              w,  W,             U214E,              U2132                   ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum

    key <AC04>	{ [         scaron,  Scaron,        U0242,              U0241                   ] };    // "Å¡", "Å ", Lowercase Glottal Stop, Uppercase Glottal Stop
    key <AC10>	{ [        eogonek,  Eogonek,       dead_macron,        dead_caron              ] };
    key <AC11>	{ [      eabovedot,  Eabovedot,     dead_doubleacute,   dead_doublegrave        ] };
    key <BKSL>	{ [              q,  Q,             bar                 ] };

    key <LSGT>	{ [           less,  greater,       endash,             dead_stroke             ] };
    key <AB02>	{ [        umacron,  Umacron,       UA78C,              UA78B                   ] };    // "Å«", "Åª", Lowercase Saltillo, Uppercase Saltillo
    key <AB08>	{ [         ccaron,  Ccaron,        dead_belowcomma     ] };
    key <AB09>	{ [              f,  F,             dead_abovedot,      dead_belowdot           ] };
    key <AB10>	{ [     braceleft,   backslash,     braceright,         dead_hook               ] };
};

partial alphanumeric_keys
xkb_symbols "dvorak" {
    // [NOTE]: Keys are arranged mostly in the same way as
    // in the Spanish Dvorak keyboard.

    // [KEYBOARD LAYOUT REFERENCE]:
    // â”Œâ”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”¬â”€â”€â”€â”€â”€â”²â”â”â”â”â”â”â”â”â”â”“
    // â”‚ Âª â—Œ â”‚ ! ğ¤€ â”‚ " áµ… â”‚ Â·   â”‚ $ Â¤ â”‚ % â‚¬ â”‚ &   â”‚ /   â”‚ ( â†» â”‚ )   â”‚ = â†º â”‚ ? É â”‚ Â¿ ê‹ â”ƒ âŒ«       â”ƒ
    // â”‚ Âº \ â”‚ 1 | â”‚ 2 @ â”‚ 3 # â”‚ 4 â—ŒÌƒ â”‚ 5  á· â”‚ 6 Â¬ â”‚ 7 â—ŒÌ¸ â”‚ 8 âˆ â”‚ 9  á·˜ â”‚ 0  Íš â”‚ ' É‚ â”‚ Â¡ êŒ â”ƒBackspaceâ”ƒ
    // â”¢â”â”â”â”â”â”·â”â”±â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”´â”€â”¬â”€â”€â”€â”ºâ”â”³â”â”â”â”â”â”â”â”«
    // â”ƒ       â”ƒ : â—ŒÌ£ â”‚ ;   â”‚ Ã‘ â—ŒÌŒ â”‚ P â†ƒ â”‚ Y êŸš â”‚ F   â”‚ G Æ¢ â”‚ C â—ŒÌ‘ â”‚ H â±µ â”‚ L Å â”‚ â—ŒÌ‚ â—ŒÌ â”‚ * â—ŒÌ› â”ƒ Enter â”ƒ
    // â”ƒ Tab â†¹ â”ƒ . â—ŒÌ‡ â”‚ , â—ŒÌ¦ â”‚ Ã± â—ŒÌ„ â”‚ p â†„ â”‚ y êŸ› â”‚ f   â”‚ g Æ£ â”‚ c â—ŒÌ§ â”‚ h â±¶ â”‚ l Å‚ â”‚ â—ŒÌ€ [ â”‚ + ] â”ƒ   â   â”ƒ
    // â”£â”â”â”â”â”â”â”â”»â”±â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”´â”¬â”€â”€â”€â”€â”ºâ”“      â”ƒ
    // â”ƒ Caps   â”ƒ A Ã… â”‚ O Ã˜ â”‚ E Æ â”‚ U Å® â”‚ I Ä° â”‚ D Ã â”‚ R É» â”‚ T Ã â”‚ N ÅŠ â”‚ S áº â”‚ â—ŒÌˆ â—ŒÌ‹ â”‚ Ã‡ â—ŒÌ¨ â”ƒ      â”ƒ
    // â”ƒ  Lock  â”ƒ a Ã¥ â”‚ o Ã¸ â”‚ e É™ â”‚ u Å¯ â”‚ i Ä± â”‚ d Ã° â”‚ r É¹ â”‚ t Ã¾ â”‚ n Å‹ â”‚ s ÃŸ â”‚ â—ŒÌ { â”‚ Ã§ } â”ƒ      â”ƒ
    // â”£â”â”â”â”â”â”â”³â”â”»â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”¬â”€â”´â”€â”€â”€â”²â”â”»â”â”â”â”â”â”»â”â”â”â”â”â”â”«
    // â”ƒ      â”ƒ > Ã† â”‚ _ â—ŒÌ‰ â”‚ Q   â”‚ J ÉŸ â”‚ K   â”‚ X Æ§ â”‚ B âŒ¬ â”‚ Ğœ  Í› â”‚ W â„² â”‚ V â—ŒÌŠ â”‚ Z Æ· â”ƒ              â”ƒ
    // â”ƒShift â”ƒ < Ã¦ â”‚ - â—ŒÌµ â”‚ q   â”‚ j È· â”‚ k   â”‚ x Æ¨ â”‚ b â€» â”‚ m â—ŒÌ¡Ì¢ â”‚ w â… â”‚ v â—ŒÌ† â”‚ z Ê’ â”ƒ Shift â‡§      â”ƒ
    // â”£â”â”â”â”â”â”â”»â”³â”â”â”â”â”·â”â”â”³â”â”â”·â”â”â”â”â”±â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”€â”´â”€â”€â”€â”€â”²â”·â”â”â”â”â”â”·â”³â”â”â”â”â”»â”â”â”³â”â”â”â”â”â”â”â”³â”â”â”â”›
    // â”ƒ       â”ƒ       â”ƒ       â”ƒ                                   â”ƒ       â”ƒ       â”ƒ â„     â”ƒ
    // â”ƒ Ctrl  â”ƒ Meta  â”ƒ  Alt  â”ƒ                Space              â”ƒAltGr â‡®â”ƒ Menu  â”ƒ Ctrl  â”ƒ
    // â”—â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”¹â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”ºâ”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”»â”â”â”â”â”â”â”â”›

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (Dvorak)";

    key <AE04>	{ [          4,  dollar,            dead_tilde,         dead_currency       ] };
    key <AE05>	{ [          5,  percent,           U1DCF,              EuroSign            ] };    // U+1DCF = "Letter Fusion" Dead Key

    key <AD01>	{ [     period,  colon,             dead_abovedot,      dead_belowdot       ] };
    key <AD02>	{ [      comma,  semicolon,         dead_belowcomma     ] };
    key <AD03>	{ [     ntilde,  Ntilde,            dead_macron,        dead_caron          ] };
    key <AD04>	{ [          p,  P,                 U2184,              U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AD05>	{ [          y,  Y,                 UA7DB,              UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AD06>	{ [          f,  F                  ] };
    key <AD07>	{ [          g,  G,                 U01A3,              U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AD08>	{ [          c,  C,                 dead_cedilla,       dead_invertedbreve  ] };
    key <AD09>	{ [          h,  H,                 U2C76,              U2C75               ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AD10>	{ [          l,  L,                 lstroke,            Lstroke             ] };
	
    key <AC02>	{ [          o,  O,                 oslash,             Oslash              ] };
    key <AC03>	{ [          e,  E,                 schwa,              SCHWA               ] };
    key <AC04>	{ [          u,  U,                 uring,              Uring               ] };
    key <AC05>	{ [          i,  I,                 idotless,           Iabovedot           ] };
    key <AC06>	{ [          d,  D,                 eth,                ETH                 ] };
    key <AC07>	{ [          r,  R,                 U0279,              U027B               ] };    // "r", "R", "É¹Ì›", "É»Ì›"
    key <AC08>	{ [          t,  T,                 thorn,              THORN               ] };
    key <AC09>	{ [          n,  N,                 eng,                ENG                 ] };
    key <AC10>	{ [          s,  S,                 ssharp,             U1E9E               ] };    // U+1E9E = Uppercase Eszett

    key <AB01>	{ [      minus,  underscore,        dead_stroke,        dead_hook           ] };
    key <AB02>	{ [          q,  Q                  ] };
    key <AB03>	{ [          j,  J,                 U0237,              U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AB04>	{ [          k,  K                  ] };
    key <AB05>	{ [          x,  X,                 U01A8,              U01A7               ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AB06>	{ [          b,  B,                 U203B,              U232C               ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
    key <AB08>	{ [          w,  W,                 U214E,              U2132               ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum
    key <AB09>	{ [          v,  V,                 dead_breve,         dead_abovering      ] };
    key <AB10>	{ [          z,  Z,                 ezh,                EZH                 ] };
};

