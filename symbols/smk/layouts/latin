    key <AB10>	{ [          z,  Z,                 ezh,                EZH                 ] };
};

partial alphanumeric_keys
xkb_symbols "colemak" {
    // [NOTE]: Keys are arranged mostly in the same way as
    // in the Spanish Colemak keyboard.

    // [KEYBOARD LAYOUT REFERENCE]:
    // ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
    // │ ª ◌ │ ! 𐤀 │ " ᵅ │ ·   │ $ ¤ │ % € │ &   │ /   │ ( ↻ │ )   │ = ↺ │ ? Ɂ │ ¿ Ꞌ ┃ ⌫       ┃
    // │ º \ │ 1 | │ 2 @ │ 3 # │ 4 ◌̃ │ 5  ᷏ │ 6 ¬ │ 7 ◌̸ │ 8 ∝ │ 9  ᷘ │ 0  ͚ │ ' ɂ │ ¡ ꞌ ┃Backspace┃
    // ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
    // ┃       ┃ Q   │ W Ⅎ │ F   │ P Ↄ │ G Ƣ │ J ɟ │ L Ł │ U Ů │ Y Ꟛ │ Ñ ◌̌ │ ◌̂ ◌̏ │ * ◌̛ ┃ Enter ┃
    // ┃ Tab ↹ ┃ q   │ w ⅎ │ f   │ p ↄ │ g ƣ │ j ȷ │ l ł │ u ů │ y ꟛ │ ñ ◌̄ │ ◌̀ [ │ + ] ┃   ⏎   ┃
    // ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
    // ┃ Caps   ┃ A Å │ R ɻ │ S ẞ │ T Þ │ D Ð │ H Ⱶ │ N Ŋ │ E Ə │ I İ │ O Ø │ ◌̈ ◌̋ │ Ç ◌̨ ┃      ┃
    // ┃  Lock  ┃ a å │ r ɹ │ s ß │ t þ │ d ð │ h ⱶ │ n ŋ │ e ə │ i ı │ o ø │ ◌́ { │ ç } ┃      ┃
    // ┣━━━━━━┳━┻───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┲━┻━━━━━┻━━━━━━┫
    // ┃      ┃ > Æ │ Z Ʒ │ X Ƨ │ C ◌̑ │ V ◌̊ │ B ⌬ │ K   │ М  ͛ │ ;   │ : ◌̣ │ _ ◌̉ ┃              ┃
    // ┃Shift ┃ < æ │ z ʒ │ x ƨ │ c ◌̧ │ v ◌̆ │ b ※ │ k   │ m ◌̡̢ │ , ◌̦ │ . ◌̇ │ - ◌̵ ┃ Shift ⇧      ┃
    // ┣━━━━━━┻┳━━━━┷━━┳━━┷━━━━┱┴─────┴─────┴─────┴─────┴─────┴────┲┷━━━━━┷┳━━━━┻━━┳━━━━━━━┳━━━┛
    // ┃       ┃       ┃       ┃                                   ┃       ┃       ┃ ⎄     ┃
    // ┃ Ctrl  ┃ Meta  ┃  Alt  ┃                Space              ┃AltGr ⇮┃ Menu  ┃ Ctrl  ┃
    // ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━━┻━━━━━━━┻━━━━━━━┛

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (Colemak)";

    key <AE04>	{ [          4,  dollar,            dead_tilde,         dead_currency       ] };
    key <AE05>	{ [          5,  percent,           U1DCF,              EuroSign            ] };    // U+1DCF = "Letter Fusion" Dead Key

    key <AD03>	{ [          f,  F                  ] };
    key <AD04>	{ [          p,  P,                 U2184,              U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AD05>	{ [          g,  G,                 U01A3,              U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AD06>	{ [          j,  J,                 U0237,              U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AD07>	{ [          l,  L,                 lstroke,            Lstroke             ] };
    key <AD08>	{ [          u,  U,                 uring,              Uring               ] };
    key <AD09>	{ [          y,  Y,                 UA7DB,              UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AD10>	{ [     ntilde,  Ntilde,            dead_macron,        dead_caron          ] };

    key <AC02>	{ [          r,  R,                 U0279,              U027B               ] };    // "r", "R", "ɹ̛", "ɻ̛"
    key <AC03>	{ [          s,  S,                 ssharp,             U1E9E               ] };    // U+1E9E = Uppercase Eszett
    key <AC04>	{ [          t,  T,                 thorn,              THORN               ] };
    key <AC05>	{ [          d,  D,                 eth,                ETH                 ] };
    key <AC06>	{ [          h,  H,                 U2C76,              U2C75               ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AC07>	{ [          n,  N,                 eng,                ENG                 ] };
    key <AC08>	{ [          e,  E,                 schwa,              SCHWA               ] };
    key <AC09>	{ [          i,  I,                 idotless,           Iabovedot           ] };
    key <AC10>	{ [          o,  O,                 oslash,             Oslash              ] };

    key <AB06>	{ [          k,  K                  ] };
};

partial alphanumeric_keys
xkb_symbols "jcuken" {
    // [NOTE]: This layout is based off a mixture between
    // the Russian ЙЦУКЕН keyboard (its approximate Latin-based form)
    // and the Spanish QWERTY one with some
    // custom rearrangements.

    // [KEYBOARD LAYOUT REFERENCE (Expanded from SMK v15.x)]:
    // ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
    // │ ª ◌ │ ! 𐤀 │ " ᵅ │ ·   │ $   │ %   │ &   │ /   │ ( ↻ │ )   │ = ↺ │ ?   │ ¿   ┃ ⌫       ┃
    // │ º \ │ 1 | │ 2 @ │ 3 # │ 4 ◌̃ │ 5  ᷏ │ 6 ¬ │ 7 ◌̸ │ 8 ∝ │ 9  ᷘ │ 0  ͚ │ '   │ ¡   ┃Backspace┃
    // ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
    // ┃       ┃ J ɟ │ C Č │ U Ů │ K   │ E ¤ │ N Ŋ │ G Ƣ │ Z Ž │ X Ƨ │ Ɂ Ꞌ │ ◌̂ ◌̏ │ * ◌̛ ┃ Enter ┃
    // ┃ Tab ↹ ┃ j ȷ │ c č │ u ů │ k   │ e € │ n ŋ │ g ƣ │ z ž │ x ƨ │ ɂ ꞌ │ ◌̀ [ │ + ] ┃   ⏎   ┃
    // ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
    // ┃ Caps   ┃ F   │ I İ │ V ◌̊ │ A Å │ P Ↄ │ R ɻ │ O Ø │ L Ł │ ẞ Ʒ │ Q ◌̌ │ ◌̈ ◌̋ │ Ñ ◌̨ ┃      ┃
    // ┃  Lock  ┃ f   │ i ı │ v ◌̆ │ a å │ p ↄ │ r ɹ │ o ø │ l ł │ ß ʒ │ q ◌̄ │ ◌́ { │ ñ } ┃      ┃
    // ┣━━━━━━┳━┻───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┲━┻━━━━━┻━━━━━━┫
    // ┃      ┃ > Æ │ Y Ꟛ │ Ç ◌̑ │ S Š │ М  ͛ │ Ɪ Ɩ │ T Þ │ B ⌬ │ ;   │ : ◌̣ │ _ ◌̉ ┃              ┃
    // ┃Shift ┃ < æ │ y ꟛ │ ç ◌̧ │ s š │ m ◌̡̢ │ ɪ ɩ │ t þ │ b ※ │ , ◌̦ │ . ◌̇ │ - ◌̵ ┃ Shift ⇧      ┃
    // ┣━━━━━━┻┳━━━━┷━━┳━━┷━━━━┱┴─────┴─────┴─────┴─────┴─────┴────┲┷━━━━━┷┳━━━━┻━━┳━━━━━━━┳━━━┛
    // ┃       ┃       ┃       ┃                                   ┃       ┃       ┃ ⎄     ┃
    // ┃ Ctrl  ┃ Meta  ┃  Alt  ┃                Space              ┃AltGr ⇮┃ Menu  ┃ Ctrl  ┃
    // ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━━┻━━━━━━━┻━━━━━━━┛

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (JCUKEN)";

    key <AE11>	{ [ apostrophe,  question           ] };
    key <AE12>	{ [ exclamdown,  questiondown       ] };

    key <AD01>	{ [          j,  J,                 U0237,                  U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AD02>	{ [          c,  C,                 ccaron,                 Ccaron              ] };
    key <AD03>	{ [          u,  U,                 uring,                  Uring               ] };
    key <AD04>	{ [          k,  K                  ] };
    key <AD05>	{ [          e,  E,                 EuroSign,               dead_currency       ] };
    key <AD06>	{ [          n,  N,                 eng,                    ENG                 ] };
    key <AD07>	{ [          g,  G,                 U01A3,                  U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AD08>	{ [          z,  Z,                 zcaron,                 Zcaron              ] };
    key <AD09>	{ [          x,  X,                 U01A8,                  U01A7               ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AD10>	{ [     U0242,   U0241,             UA78C,                  UA78B               ] };    // Lowercase Glottal Stop, Uppercase Glottal Stop, Lowercase Saltillo, Uppercase Saltillo 

    key <AC01>	{ [          f,  F                  ] };
    key <AC02>	{ [          i,  I,                 idotless,               Iabovedot           ] };
    key <AC03>	{ [          v,  V,                 dead_breve,             dead_abovering      ] };
    key <AC04>	{ [          a,  A,                 aring,                  Aring               ] };
    key <AC05>	{ [          p,  P,                 U2184,                  U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AC06>	{ [          r,  R,                 U0279,                  U027B               ] };    // "r", "R", "ɹ̛", "ɻ̛"
    key <AC07>	{ [          o,  O,                 oslash,                 Oslash              ] };
    key <AC08>	{ [          l,  L,                 lstroke,                Lstroke             ] };
    key <AC09>	{ [     ssharp,  U1E9E,             ezh,                    EZH                 ] };    // U+1E9E = Uppercase Eszett
    key <AC10>	{ [          q,  Q,                 dead_macron,            dead_caron          ] };
    key <BKSL>	{ [     ntilde,  Ntilde,            braceright,             dead_ogonek         ] };

    key <AB01>	{ [          y,  Y,                 UA7DB,                  UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AB02>	{ [   ccedilla,  Ccedilla,          dead_cedilla,           dead_invertedbreve  ] };
    key <AB03>	{ [          s,  S,                 scaron,                 Scaron              ] };    // U+1E9E = Uppercase Eszett
    key <AB04>	{ [          m,  M,                 U0359,                  U035B               ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AB05>	{ [      U026A,  UA7AE,             U0269,                  U0196               ] };    // Lowercase Smallcaps I, Uppercase Smallcaps I, Lowercase Latin Iota, Uppercase Latin Iota
    key <AB06>	{ [          t,  T,                 thorn,                  THORN               ] };
    key <AB07>	{ [          b,  B,                 U203B,                  U232C               ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
};

partial alphanumeric_keys
xkb_symbols "turkish" {
    // [NOTE]: Keys are arranged the same way as
    // in the standard Spanish QWERTY keyboard.

    // [KEYBOARD LAYOUT REFERENCE]:
    // ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
    // │ * ◌ │ ! ¡ │ " ᵅ │ ◌̂ ³ │ $ ¤ │ % £ │ & ª │ ' ◌̸ │ ( ↻ │ )  ᷘ │ = ↺ │ ? ¿ │ _ ◌̉ ┃ ⌫       ┃
    // │ + ¬ │ 1 ¹ │ 2 ² │ 3 # │ 4 ₺ │ 5  ᷏ │ 6 [ │ 7 { │ 8 } │ 9 ] │ 0  ͚ │ / \ │ - ◌̵ ┃Backspace┃
    // ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
    // ┃       ┃ F º │ G Ƣ │ Ğ Ɂ │ I İ │ O Ø │ D Ð │ R ɻ │ N Ŋ │ H Ⱶ │ P Ↄ │ Q ◌̊ │ W Ⅎ ┃ Enter ┃
    // ┃ Tab ↹ ┃ f @ │ g ƣ │ ğ ɂ │ ı i │ o ø │ d ð │ r ɹ │ n ŋ │ h ⱶ │ p ↄ │ q ◌̈ │ w ⅎ ┃   ⏎   ┃
    // ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
    // ┃ Caps   ┃ U Ů │ ẞ Ʒ │ E Ə │ A Æ │ Ü Ꞌ │ T Þ │ K ◌̌ │ М  ͛ │ L Ł │ Y Ꟛ │ ◌̀ ◌̏ │ X Ƨ ┃      ┃
    // ┃  Lock  ┃ u ů │ ß ʒ │ e ə │ a æ │ ü ꞌ │ t þ │ k ◌̄ │ m ◌̡̢ │ l ł │ y ꟛ │ ◌́ ◌̋ │ x ƨ ┃      ┃
    // ┣━━━━━━┳━┻───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┲━┻━━━━━┻━━━━━━┫
    // ┃      ┃ > ¦ │ J ɟ │ Ö Œ │ V ◌̑ │ C Ç │ ◌̃ ◌̛ │ Z   │ S Ş │ B ⌬ │ : ◌̣ │ ; ∝ ┃              ┃
    // ┃Shift ┃ < | │ j ȷ │ ö œ │ v ◌̆ │ c ç │ ◌̧ ◌̨ │ z   │ s ş │ b ※ │ . ◌̇ │ , ◌̦ ┃ Shift ⇧      ┃
    // ┣━━━━━━┻┳━━━━┷━━┳━━┷━━━━┱┴─────┴─────┴─────┴─────┴─────┴────┲┷━━━━━┷┳━━━━┻━━┳━━━━━━━┳━━━┛
    // ┃       ┃       ┃       ┃                                   ┃       ┃       ┃ ⎄     ┃
    // ┃ Ctrl  ┃ Meta  ┃  Alt  ┃                Space              ┃AltGr ⇮┃ Menu  ┃ Ctrl  ┃
    // ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━━┻━━━━━━━┻━━━━━━━┛

    include "smk/latin(basic)"

    name[Group1]= "SMK-Latin (FGĞIOD)";

    key <TLDE>	{ [         plus,   asterisk,           notsign,              U25CC                     ] };    // U+25CC = Dotted Circle
    key <AE01>	{ [            1,   exclam,             onesuperior,          exclamdown                ] };
    key <AE02>	{ [            2,   quotedbl,           twosuperior,          dead_greek                ] };
    key <AE03>	{ [            3,   dead_circumflex,    numbersign,           threesuperior             ] };
    key <AE04>	{ [            4,   dollar,             LiraSign,             dead_currency             ] };
    key <AE05>	{ [            5,   percent,            U1DCF,                sterling                  ] };    // U+1DCF = "Letter Fusion" Dead Key
    key <AE06>	{ [            6,   ampersand,          bracketleft,          ordfeminine               ] };
    key <AE07>	{ [            7,   apostrophe,         braceleft,            dead_longsolidusoverlay   ] };
    key <AE08>	{ [            8,   parenleft,          braceright,           U21BB                     ] };    // U+221D = "Logic + Math Symbols" Dead Key; U+21BB = "Arrows" Dead Key
    key <AE11>	{ [        slash,   question,           backslash,            questiondown              ] };
    key <AE12>	{ [        minus,   underscore,         dead_stroke,          dead_hook                 ] };

    key <AD01>	{ [            f,   F,                  at,                   masculine                 ] };
    key <AD02>	{ [            g,   G,                  U01A3,                U01A2                     ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AD03>	{ [       gbreve,   Gbreve,             U0242,                U0241                     ] };    // "ğ", "Ğ", Lowercase Glottal Stop, Uppercase Glottal Stop 
    key <AD04>	{ [     idotless,   I,                  i,                    Iabovedot                 ] };
    key <AD05>	{ [            o,   O,                  oslash,               Oslash                    ] };
    key <AD06>	{ [            d,   D,                  eth,                  ETH                       ] };
    key <AD07>	{ [            r,   R,                  U0279,                U027B                     ] };    // "r", "R", "ɹ̛", "ɻ̛"
    key <AD08>	{ [            n,   N,                  eng,                  ENG                       ] };
    key <AD09>	{ [            h,   H,                  U2C76,                U2C75                     ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AD11>	{ [            q,   Q,                  dead_diaeresis,       dead_abovering            ] };
    key <AD12>	{ [            w,   W,                  U214E,                U2132                     ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum

    key <AC01>	{ [            u,   U,                  uring,                Uring                     ] };
    key <AC02>	{ [       ssharp,   U1E9E,              ezh,                  EZH                       ] };    // U+1E9E = Uppercase Eszett
    key <AC03>	{ [            e,   E,                  schwa,                SCHWA                     ] };
    key <AC04>	{ [            a,   A,                  ae,                   AE                        ] };
    key <AC05>	{ [   udiaeresis,   Udiaeresis,         UA78C,                UA78B                     ] };    // "ü", "Ü", Lowercase Saltillo, Uppercase Saltillo 
    key <AC06>	{ [            t,   T,                  thorn,                THORN                     ] };
    key <AC07>	{ [            k,   K,                  dead_macron,          dead_caron                ] };
    key <AC08>	{ [            m,   M,                  U0359,                U035B                     ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AC10>	{ [            y,   Y,                  UA7DB,                UA7DA                     ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AC11>	{ [   dead_acute,   dead_grave,         dead_doubleacute,     dead_doublegrave          ] };
    key <BKSL>	{ [            x,   X,                  U01A8,                U01A7                     ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two

    key <LSGT>	{ [         less,   greater,            bar,                  brokenbar                 ] };
    key <AB01>	{ [            j,   J,                  U0237,                U025F                     ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AB02>	{ [   odiaeresis,   Odiaeresis,         oe,                   OE                        ] };
    key <AB03>	{ [            v,   V,                  dead_breve,           dead_invertedbreve        ] };
    key <AB04>	{ [            c,   C,                  ccedilla,             Ccedilla                  ] };
    key <AB05>	{ [ dead_cedilla,   dead_tilde,         dead_ogonek,          dead_horn                 ] };
    key <AB06>	{ [            z,   Z                   ] };
    key <AB07>	{ [            s,   S,                  scedilla,             Scedilla                  ] };
    key <AB08>	{ [            b,   B,                  U203B,                U232C                     ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
    key <AB10>	{ [        comma,   semicolon,          dead_belowcomma,      U221D                     ] };    // U+221D = "Logic + Math Symbols" Dead Key
};

partial alphanumeric_keys
xkb_symbols "hcesar" {
    // [NOTE]: This layout is mostly based off
    // the Portuguese HCESAR keyboard
    // with some custom rearrangements and additions.

    // [KEYBOARD LAYOUT REFERENCE]:
    // ┌─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┬─────┲━━━━━━━━━┓
    // │ ª  ͚ │ !   │ " ᵅ │ # · │ $   │ %   │ &   │ / ◌̸ │ ( ∝ │ ) ↻ │ = ↺ │ ? Ɂ │ » ¿ ┃ ⌫       ┃
    // │ º  ᷘ │ 1 𐤟 │ 2 @ │ 3 £ │ 4 § │ 5  ᷏ │ 6 ¬ │ 7 { │ 8 [ │ 9 ] │ 0 } │ ' ɂ │ « ¡ ┃Backspace┃
    // ┢━━━━━┷━┱───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┴─┬───┺━┳━━━━━━━┫
    // ┃       ┃ _ ◌̉ │ H Ⱶ │ C ◌̑ │ E ¤ │ S ẞ │ A Å │ R ɻ │ O Ø │ P Ↄ │ Z Ʒ │ * ◌ │ ◌̀ ◌̏ ┃ Enter ┃
    // ┃ Tab ↹ ┃ - ◌̵ │ h ⱶ │ c ◌̧ │ e € │ s ß │ a å │ r ɹ │ o ø │ p ↄ │ z ʒ │ + ◌̈ │ ◌́ ◌̋ ┃   ⏎   ┃
    // ┣━━━━━━━┻┱────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┴┬────┺┓      ┃
    // ┃ Caps   ┃ | Ꞌ │ Q   │ T Þ │ D Ð │ I İ │ N Ŋ │ U Ů │ L Ł │ М  ͛ │ X Ƨ │ ; ◌̓ │ ◌̂ ◌̨ ┃      ┃
    // ┃  Lock  ┃ \ ꞌ │ q   │ t þ │ d ð │ i ı │ n ŋ │ u ů │ l ł │ m ◌̡̢ │ x ƨ │ , ◌̦ │ ◌̃ ◌̛ ┃      ┃
    // ┣━━━━━━┳━┻───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┬─┴───┲━┻━━━━━┻━━━━━━┫
    // ┃      ┃ > Æ │ Y Ꟛ │ Ç ◌̌ │ J ɟ │ B ⌬ │ F   │ V ◌̊ │ G Ƣ │ K   │ W Ⅎ │ : ◌̣ ┃              ┃
    // ┃Shift ┃ < æ │ y ꟛ │ ç ◌̄ │ j ȷ │ b ※ │ f   │ v ◌̆ │ g ƣ │ k   │ w ⅎ │ . ◌̇ ┃ Shift ⇧      ┃
    // ┣━━━━━━┻┳━━━━┷━━┳━━┷━━━━┱┴─────┴─────┴─────┴─────┴─────┴────┲┷━━━━━┷┳━━━━┻━━┳━━━━━━━┳━━━┛
    // ┃       ┃       ┃       ┃                                   ┃       ┃       ┃ ⎄     ┃
    // ┃ Ctrl  ┃ Meta  ┃  Alt  ┃                Space              ┃AltGr ⇮┃ Menu  ┃ Ctrl  ┃
    // ┗━━━━━━━┻━━━━━━━┻━━━━━━━┹───────────────────────────────────┺━━━━━━━┻━━━━━━━┻━━━━━━━┛

    include "smk/latin(qwerty_pt)"

    name[Group1]= "SMK-Latin (HCESAR)";

    key <TLDE>	{ [  masculine,  ordfeminine,       U1DCF,                  U035A               ] };     // U+1DCF = "Letter Fusion" Dead Key; U+035A = "IPA Symbols" Dead Key

    key <AD01>	{ [      minus,  underscore,        dead_stroke,            dead_hook           ] };
    key <AD02>	{ [          h,  H,                 U2C76,                  U2C75               ] };    // "h", "H", Lowercase Sonus Medius, Uppercase Sonus Medius
    key <AD03>	{ [          c,  C,                 dead_cedilla,           dead_invertedbreve  ] };
    key <AD04>	{ [          e,  E,                 EuroSign,               dead_currency       ] };
    key <AD05>	{ [          s,  S,                 ssharp,                 U1E9E               ] };    // U+1E9E = Uppercase Eszett
    key <AD06>	{ [          a,  A,                 aring,                  Aring               ] };
    key <AD07>	{ [          r,  R,                 U0279,                  U027B               ] };    // "r", "R", "ɹ̛", "ɻ̛"
    key <AD08>	{ [          o,  O,                 oslash,                 Oslash              ] };
    key <AD09>	{ [          p,  P,                 U2184,                  U2183               ] };    // "p", "P", Lowercase Antisigma, Uppercase Antisigma
    key <AD10>	{ [          z,  Z,                 ezh,                    EZH                 ] };

    key <AC01>	{ [  backslash,  bar,               UA78C,                  UA78B               ] };    // "\", "|", Lowercase Saltillo, Uppercase Saltillo 
    key <AC02>	{ [          q,  Q                  ] };
    key <AC03>	{ [          t,  T,                 thorn,                  THORN               ] };
    key <AC04>	{ [          d,  D,                 eth,                    ETH                 ] };
    key <AC05>	{ [          i,  I,                 idotless,               Iabovedot           ] };
    key <AC06>	{ [          n,  N,                 eng,                    ENG                 ] };
    key <AC07>	{ [          u,  U,                 uring,                  Uring               ] };
    key <AC08>	{ [          l,  L,                 lstroke,                Lstroke             ] };
    key <AC09>	{ [          m,  M,                 U0359,                  U035B               ] };    // U+0359 = "Hooktail" Dead Key, U+035B = "Curly Loop" Dead Key
    key <AC10>	{ [          x,  X,                 U01A8,                  U01A7               ] };    // "x", "X", Lowercase Tone Two, Uppercase Tone Two
    key <AC11>	{ [      comma,  semicolon,         dead_belowcomma         ] };

    key <AB01>	{ [          y,  Y,                 UA7DB,                  UA7DA               ] };    // "y", "Y", Lowercase Latin Lambda, Uppercase Latin Lambda
    key <AB02>	{ [   ccedilla,  Ccedilla,          dead_macron,            dead_caron          ] };
    key <AB03>	{ [          j,  J,                 U0237,                  U025F               ] };    // "j", "J", Lowercase Dotless J, Lowercase Barred Dotless J
    key <AB04>	{ [          b,  B,                 U203B,                  U232C               ] };    // U+203B = "Punctuation Marks" Dead Key; U+232C = "Miscellaneous Symbols" Dead Key
    key <AB05>	{ [          f,  F                  ] };
    key <AB06>	{ [          v,  V,                 dead_breve,             dead_abovering      ] };
    key <AB07>	{ [          g,  G,                 U01A3,                  U01A2               ] };    // "g", "G", Lowercase Gha, Uppercase Gha
    key <AB08>	{ [          k,  K                  ] };
    key <AB09>	{ [          w,  W,                 U214E,                  U2132               ] };    // "w", "W", Lowercase Digamma Inversum, Uppercase Digamma Inversum
    key <AB10>	{ [     period,  colon,             dead_abovedot,          dead_belowdot       ] };
};

