<dead_doublegrave> <UA7CB>                              : "Ɤ̏"   # Ɤ (Ram's Horn) + Double Grave
<dead_doublegrave> <U0264>                              : "ɤ̏"   # ɤ (Ram's Horn) + Double Grave
<dead_doublegrave> <U027B>                              : "ɻ̏"   # ɻ + Double Grave
<dead_doublegrave> <U0279>                              : "ɹ̏"   # ɹ + Double Grave
<dead_doublegrave> <U035A> <R>                          : "ɻ̏"   # ɻ + Double Grave
<dead_doublegrave> <U035A> <r>                          : "ɹ̏"   # ɹ + Double Grave
<dead_doublegrave> <U21BA> <R>                          : "ɻ̏"   # ɻ + Double Grave
<dead_doublegrave> <U21BA> <r>                          : "ɹ̏"   # ɹ + Double Grave
<dead_doublegrave> <U01A9>                              : "Ʃ̏"   # Ʃ (Esh) + Double Grave
