# Composed macron combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main reference]: https://fr.wikipedia.org/wiki/Macron_(diacritique)
#
<dead_macron> <Aacute>                                      : "Ā́"
<dead_macron> <aacute>                                      : "ā́"
<dead_macron> <dead_acute> <A>                              : "Ā́"
<dead_macron> <dead_acute> <a>                              : "ā́"
<dead_macron> <dead_acute> <Atilde>                         : "Ā̃́"
<dead_macron> <dead_acute> <atilde>                         : "ā̃́"
<dead_macron> <dead_acute> <dead_tilde> <A>                 : "Ā̃́"
<dead_macron> <dead_acute> <dead_tilde> <a>                 : "ā̃́"
<dead_macron> <dead_acute> <Aogonek>                        : "Ą̄́"
<dead_macron> <dead_acute> <aogonek>                        : "ą̄́"
<dead_macron> <dead_acute> <dead_ogonek> <A>                : "Ą̄́"
<dead_macron> <dead_acute> <dead_ogonek> <a>                : "ą̄́"
<dead_macron> <dead_acute> <dead_belowmacron> <A>           : "Ā̱́"
<dead_macron> <dead_acute> <dead_belowmacron> <a>           : "ā̱́"
<dead_macron> <dead_acute> <Abreve>                         : "Ā̆́"
<dead_macron> <dead_acute> <abreve>                         : "ā̆́"
<dead_macron> <dead_acute> <dead_breve> <A>                 : "Ā̆́"
<dead_macron> <dead_acute> <dead_breve> <a>                 : "ā̆́"
<dead_macron> <Agrave>                                      : "Ā̀"
<dead_macron> <agrave>                                      : "ā̀"
<dead_macron> <dead_grave> <Aogonek>                        : "Ą̄̀"
<dead_macron> <dead_grave> <aogonek>                        : "ą̄̀"
<dead_macron> <dead_grave> <dead_ogonek> <A>                : "Ą̄̀"
<dead_macron> <dead_grave> <dead_ogonek> <a>                : "ą̄̀"
<dead_macron> <dead_grave> <A>                              : "Ā̀"
<dead_macron> <dead_grave> <a>                              : "ā̀"
<dead_macron> <dead_grave> <dead_belowmacron> <A>           : "Ā̱̀"
<dead_macron> <dead_grave> <dead_belowmacron> <a>           : "ā̱̀"
<dead_macron> <Acircumflex>                                 : "Ā̂"
<dead_macron> <acircumflex>                                 : "ā̂"
<dead_macron> <dead_circumflex> <Aogonek>                   : "Ą̄̂"
<dead_macron> <dead_circumflex> <aogonek>                   : "ą̄̂"
