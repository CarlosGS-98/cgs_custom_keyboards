# Latin Alphabet | Macron Rules
# [AUTHOR]: Carlos González Sanz

# Basic precomposed letter combinations
<dead_macron> <A>                   : "Ā"
<dead_macron> <a>                   : "ā"
<dead_macron> <AE>                  : "Ǣ"
<dead_macron> <ae>                  : "ǣ"
<dead_macron> <multiply> <A> <E>    : "Ǣ"
<dead_macron> <multiply> <a> <e>    : "ǣ"
<dead_macron> <E>                   : "Ē"
<dead_macron> <e>                   : "ē"
<dead_macron> <G>                   : "Ḡ"
<dead_macron> <g>                   : "ḡ"
<dead_macron> <I>                   : "Ī"
<dead_macron> <i>                   : "ī"
<dead_macron> <O>                   : "Ō"
<dead_macron> <o>                   : "ō"
<dead_macron> <U>                   : "Ū"
<dead_macron> <u>                   : "ū"
<dead_macron> <Y>                   : "Ȳ"
<dead_macron> <y>                   : "ȳ"


# Other precomposed macron combinations (Unidirectional)
<dead_macron> <Adiaeresis>          : "Ǟ"
<dead_macron> <adiaeresis>          : "ǟ"
<dead_macron> <dead_diaeresis> <A>  : "Ǟ"
<dead_macron> <dead_diaeresis> <a>  : "ǟ"
<dead_macron> <U0226>               : "Ǡ"    # Ȧ + Macron
<dead_macron> <U0227>               : "ǡ"    # ȧ + Macron
<dead_macron> <dead_abovedot> <A>   : "Ǡ"
<dead_macron> <dead_abovedot> <a>   : "ǡ"
<dead_macron> <Eacute>              : "Ḗ"
<dead_macron> <eacute>              : "ḗ"
<dead_macron> <dead_acute> <E>      : "Ḗ"
<dead_macron> <dead_acute> <e>      : "ḗ"
<dead_macron> <Egrave>              : "Ḕ"
<dead_macron> <egrave>              : "ḕ"
<dead_macron> <dead_grave> <E>      : "Ḕ"
<dead_macron> <dead_grave> <e>      : "ḕ"
<dead_macron> <Lbelowdot>           : "Ḹ"
<dead_macron> <lbelowdot>           : "ḹ"
<dead_macron> <dead_belowdot> <L>   : "Ḹ"
<dead_macron> <dead_belowdot> <l>   : "ḹ"
<dead_macron> <Oacute>              : "Ṓ"
<dead_macron> <oacute>              : "ṓ"
<dead_macron> <dead_acute> <O>      : "Ṓ"
<dead_macron> <dead_acute> <o>      : "ṓ"
<dead_macron> <Ograve>              : "Ṑ"
<dead_macron> <ograve>              : "ṑ"
<dead_macron> <dead_grave> <O>      : "Ṑ"
<dead_macron> <dead_grave> <o>      : "ṑ"
<dead_macron> <Odiaeresis>          : "Ȫ"
<dead_macron> <odiaeresis>          : "ȫ"
<dead_macron> <dead_diaeresis> <O>  : "Ȫ"
<dead_macron> <dead_diaeresis> <o>  : "ȫ"
<dead_macron> <Otilde>              : "Ȭ"
<dead_macron> <otilde>              : "ȭ"
<dead_macron> <dead_tilde> <O>      : "Ȭ"
<dead_macron> <dead_tilde> <o>      : "ȭ"
<dead_macron> <U01EA>               : "Ǭ"   # Ǫ + Macron
<dead_macron> <U01EB>               : "ǭ"   # ǫ + Macron
<dead_macron> <dead_ogonek> <O>     : "Ǭ"
<dead_macron> <dead_ogonek> <o>     : "ǭ"
<dead_macron> <U022E>               : "Ȱ"   # Ȯ + Macron
<dead_macron> <U022F>               : "ȱ"   # ȯ + Macron
<dead_macron> <dead_abovedot> <O>   : "Ȱ"
<dead_macron> <dead_abovedot> <o>   : "ȱ"
<dead_macron> <U1E5A>               : "Ṝ"   # Ṛ + Macron
<dead_macron> <U1E5B>               : "ṝ"   # ṛ + Macron
<dead_macron> <dead_belowdot> <R>   : "Ṝ"
<dead_macron> <dead_belowdot> <r>   : "ṝ"
<dead_macron> <Udiaeresis>          : "Ǖ"
<dead_macron> <udiaeresis>          : "ǖ"
<dead_macron> <dead_diaeresis> <U>  : "Ǖ"
<dead_macron> <dead_diaeresis> <u>  : "ǖ"


# Composed base letter combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main reference]: https://fr.wikipedia.org/wiki/Macron_(diacritique)
#
# [CUSTOM DEAD KEYS]:
# - ↺ (Anticlockwise Open Circle Arrow [U21BA]) -> Represents the "Letter Flip" dead key
#
<dead_macron> <U2C6F>            : "Ɐ̄"      # Ɐ + Macron
<dead_macron> <U0250>            : "ɐ̄"      # ɐ + Macron
<dead_macron> <U21BA> <A>        : "Ɐ̄"      # Ɐ + Macron
<dead_macron> <U21BA> <a>        : "ɐ̄"      # ɐ + Macron
<dead_macron> <U2C6D>            : "Ɑ̄"      # Ɑ (Latin Alpha) + Macron
<dead_macron> <U0251>            : "ɑ̄"      # ɑ (Latin Alpha) + Macron
<dead_macron> <B>                : "B̄"
<dead_macron> <b>                : "b̄"
<dead_macron> <C>                : "C̄"
<dead_macron> <c>                : "c̄"
<dead_macron> <D>                : "D̄"
<dead_macron> <d>                : "d̄"
<dead_macron> <U0190>            : "Ɛ̄"      # Ɛ (Latin Epsilon) + Macron
<dead_macron> <U025B>            : "ɛ̄"      # ɛ (Latin Epsilon) + Macron
<dead_macron> <SCHWA>            : "Ə̄"      # Ə (Schwa) + Macron
<dead_macron> <schwa>            : "ə̄"      # ə (Schwa) + Macron
<dead_macron> <U018E>            : "Ǝ̄"      # Ǝ (Turned E) + Macron
<dead_macron> <U01DD>            : "ǝ̄"      # ǝ (Turned E) + Macron
<dead_macron> <U21BA> <E>        : "Ǝ̄"      # Ǝ (Turned E) + Macron
<dead_macron> <U21BA> <e>        : "ǝ̄"      # ǝ (Turned E) + Macron
<dead_macron> <F>                : "F̄"
<dead_macron> <f>                : "f̄"
<dead_macron> <H>                : "H̄"
<dead_macron> <h>                : "h̄"
<dead_macron> <UA78D>            : "Ɥ̄"      # Ɥ (Turned H) + Macron
<dead_macron> <U0265>            : "ɥ̄"      # ɥ (Turned H) + Macron
<dead_macron> <U21BA> <H>        : "Ɥ̄"      # Ɥ (Turned H) + Macron
<dead_macron> <U21BA> <h>        : "ɥ̄"      # ɥ (Turned H) + Macron
<dead_macron> <U0196>            : "Ɩ̄"      # Ɩ (Latin Iota) + Macron
<dead_macron> <U0269>            : "ɩ̄"      # ɩ (Latin Iota) + Macron
<dead_macron> <J>                : "J̄"
<dead_macron> <j>                : "j̄"
<dead_macron> <K>                : "K̄"
<dead_macron> <k>                : "k̄"
<dead_macron> <L>                : "L̄"
<dead_macron> <l>                : "l̄"
<dead_macron> <M>                : "M̄"
<dead_macron> <m>                : "m̄"
<dead_macron> <N>                : "N̄"
<dead_macron> <n>                : "n̄"
<dead_macron> <ENG>              : "Ŋ̄"      # Ŋ + Macron
<dead_macron> <eng>              : "ŋ̄"      # ŋ + Macron
<dead_macron> <OE>               : "Œ̄"
<dead_macron> <oe>               : "œ̄"
<dead_macron> <multiply> <O> <E> : "Œ̄"
<dead_macron> <multiply> <o> <e> : "œ̄"
<dead_macron> <U0186>            : "Ɔ̄"      # Ɔ + Macron
<dead_macron> <U0254>            : "ɔ̄"      # ɔ + Macron
<dead_macron> <UA7CB>            : "Ɤ̄"      # Ɤ (Ram's Horn) + Macron
<dead_macron> <U0264>            : "ɤ̄"      # ɤ (Ram's Horn) + Macron
<dead_macron> <P>                : "P̄"
<dead_macron> <p>                : "p̄"
<dead_macron> <Q>                : "Q̄"
<dead_macron> <q>                : "q̄"
<dead_macron> <R>                : "R̄"
<dead_macron> <r>                : "r̄"
<dead_macron> <S>                : "S̄"
<dead_macron> <s>                : "s̄"
<dead_macron> <T>                : "T̄"
<dead_macron> <t>                : "t̄"
<dead_macron> <U01B1>            : "Ʊ̄"     # Ʊ (Latin Upsilon) + Macron
<dead_macron> <U028A>            : "ʊ̄"     # ʊ (Latin Upsilon) + Macron
