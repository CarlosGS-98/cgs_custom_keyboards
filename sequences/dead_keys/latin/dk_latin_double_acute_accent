<dead_doubleacute> <UA739>                              : "ꜹ̋"   # ꜹ + Double Acute
<dead_doubleacute> <U1DCF> <A> <V>                      : "Ꜹ̋"  # Ꜹ + Double Acute
<dead_doubleacute> <U1DCF> <a> <v>                      : "ꜹ̋"   # ꜹ + Double Acute
<dead_doubleacute> <U2C6D>                              : "Ɑ̋"   # Ɑ (Latin Alpha) + Double Acute
<dead_doubleacute> <U0251>                              : "ɑ̋"   # ɑ (Latin Alpha) + Double Acute
<dead_doubleacute> <dead_greek> <dead_circumflex> <A>   : "Ɑ̋"   # Ɑ (Latin Alpha) + Double Acute
<dead_doubleacute> <dead_greek> <dead_circumflex> <a>   : "ɑ̋"   # ɑ (Latin Alpha) + Double Acute
<dead_doubleacute> <U2C70>                              : "Ɒ̋"   # Ɒ (Turned Latin Alpha) + Double Acute
<dead_doubleacute> <U0252>                              : "ɒ̋"   # ɒ (Turned Latin Alpha) + Double Acute
<dead_doubleacute> <U21BA> <dead_greek> <A>             : "Ɒ̋"   # Ɒ (Turned Latin Alpha) + Double Acute
<dead_doubleacute> <U21BA> <dead_greek> <a>             : "ɒ̋"   # ɒ (Turned Latin Alpha) + Double Acute
<dead_doubleacute> <UA7B4>                              : "Ꞵ̋"   # Ꞵ (Latin Beta) + Double Acute
<dead_doubleacute> <UA7B5>                              : "ꞵ̋"   # ꞵ (Latin Beta) + Double Acute
