# Composed double acute accent combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main source]: https://fr.wikipedia.org/wiki/Double_accent_aigu
#
# [CUSTOM DEAD KEYS]:
# - ᷎ (Combining Ogonek Above [U1DCE]) -> Represents the "Ogonek Above" dead key
# - ↺ (Anticlockwise Open Circle Arrow [U21BA]) -> Represents the "Letter Flip" dead key
#
<dead_doubleacute> <Aogonek>                            : "Ą̋"
<dead_doubleacute> <aogonek>                            : "ą̋"
<dead_doubleacute> <dead_ogonek> <A>                    : "Ą̋"
<dead_doubleacute> <dead_ogonek> <a>                    : "ą̋"
<dead_doubleacute> <Ecaron>                             : "Ě̋"
<dead_doubleacute> <ecaron>                             : "ě̋"
<dead_doubleacute> <dead_caron> <E>                     : "Ě̋"
<dead_doubleacute> <dead_caron> <e>                     : "ě̋"
<dead_doubleacute> <Eogonek>                            : "Ę̋"
<dead_doubleacute> <eogonek>                            : "ę̋"
<dead_doubleacute> <dead_ogonek> <E>                    : "Ę̋"
<dead_doubleacute> <dead_ogonek> <e>                    : "ę̋"
<dead_doubleacute> <Iogonek>                            : "Į̋"
<dead_doubleacute> <iogonek>                            : "į̋"
<dead_doubleacute> <dead_ogonek> <I>                    : "Į̋"
<dead_doubleacute> <dead_ogonek> <i>                    : "į̋"
<dead_doubleacute> <U0197>                              : "Ɨ̋"   # Ɨ + Double Acute
<dead_doubleacute> <U0268>                              : "ɨ̋"   # ɨ + Double Acute
<dead_doubleacute> <dead_stroke> <I>                    : "Ɨ̋"
<dead_doubleacute> <dead_stroke> <i>                    : "ɨ̋"
<dead_doubleacute> <dead_stroke> <Idotabove>            : "Ɨ̋"
<dead_doubleacute> <dead_stroke> <idotless>             : "ɨ̋"
<dead_doubleacute> <U01EA>                              : "Ǫ̋"   # Ǫ + Double Acute Accent
<dead_doubleacute> <U01EB>                              : "ǫ̋"   # ǫ + Double Acute Accent
<dead_doubleacute> <dead_ogonek> <O>                    : "Ǫ̋"
<dead_doubleacute> <dead_ogonek> <o>                    : "ǫ̋"
<dead_doubleacute> <Omacron>                            : "Ō̋"
<dead_doubleacute> <omacron>                            : "ō̋"
<dead_doubleacute> <dead_macron> <O>                    : "Ō̋"
<dead_doubleacute> <dead_macron> <o>                    : "ō̋"
<dead_doubleacute> <U019F>                              : "Ɵ̋"   # Ɵ + Double Acute
<dead_doubleacute> <U0275>                              : "ɵ̋"   # ɵ + Double Acute
<dead_doubleacute> <dead_stroke> <O>                    : "Ɵ̋"
<dead_doubleacute> <dead_stroke> <o>                    : "ɵ̋"
<dead_doubleacute> <Oslash>                             : "Ø̋"
<dead_doubleacute> <oslash>                             : "ø̋"
<dead_doubleacute> <dead_longsolidusoverlay> <O>        : "Ø̋"
<dead_doubleacute> <dead_longsolidusoverlay> <o>        : "ø̋"
<dead_doubleacute> <Uogonek>                            : "Ų̋"
<dead_doubleacute> <uogonek>                            : "ų̋"
<dead_doubleacute> <dead_ogonek> <U>                    : "Ų̋"
<dead_doubleacute> <dead_ogonek> <u>                    : "ų̋"
<dead_doubleacute> <Umacron>                            : "Ū̋"
<dead_doubleacute> <umacron>                            : "ū̋"
<dead_doubleacute> <dead_macron> <U>                    : "Ū̋"
<dead_doubleacute> <dead_macron> <u>                    : "ū̋"
<dead_doubleacute> <U0244>                              : "Ʉ̋"   # Ʉ + Double Acute
<dead_doubleacute> <U0289>                              : "ʉ̋"   # ʉ + Double Acute
<dead_doubleacute> <dead_stroke> <U>                    : "Ʉ̋"
<dead_doubleacute> <dead_stroke> <u>                    : "ʉ̋"
<dead_doubleacute> <UA7B8>                              : "Ꞹ̋"   # Ꞹ + Double Acute
<dead_doubleacute> <UA7B9>                              : "ꞹ̋"   # ꞹ + Double Acute
<dead_doubleacute> <dead_longsolidusoverlay> <U>        : "Ꞹ̋"
<dead_doubleacute> <dead_longsolidusoverlay> <u>        : "ꞹ̋"
<dead_doubleacute> <U02A1>                              : "ʡ̋"   # Ɂ (Capital Glottal Stop) + "-" + Double Acute
<dead_doubleacute> <U035A> <U0241> <minus>              : "ʡ̋"   # Ɂ (Capital Glottal Stop) + "-" + Double Acute
<dead_doubleacute> <U035A> <U0242> <minus>              : "ʡ̋"   # ɂ (Small Glottal Stop) + "-" + Double Acute
<dead_doubleacute> <U02A2>                              : "ʢ̋"   # ʢ (Barred Turned Glottal Stop) + Double Acute
<dead_doubleacute> <U035A> <U0241> <underscore> <minus> : "ʢ̋"   # Ɂ (Capital Glottal Stop) + "_-" + Double Acute
<dead_doubleacute> <U035A> <U0242> <underscore> <minus> : "ʢ̋"   # ɂ (Small Glottal Stop) + "_-" + Double Acute

