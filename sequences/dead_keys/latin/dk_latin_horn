# Composed horn combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main reference]: https://fr.wikipedia.org/wiki/Cornu_(diacritique)
#
<dead_horn> <Omacron>                       : "Ơ̄"
<dead_horn> <omacron>                       : "ơ̄"
<dead_horn> <dead_macron> <O>               : "Ơ̄"
<dead_horn> <dead_macron> <o>               : "ơ̄"
<dead_horn> <U014E>                         : "Ơ̆"   # Ŏ + Horn
<dead_horn> <U014F>                         : "ơ̆"   # ŏ + Horn
<dead_horn> <dead_breve> <O>                : "Ơ̆"
<dead_horn> <dead_breve> <o>                : "ơ̆"
<dead_horn> <Oslash>                        : "Ø̛"
<dead_horn> <oslash>                        : "ø̛"
<dead_horn> <dead_longsolidusoverlay> <O>   : "Ø̛"
<dead_horn> <dead_longsolidusoverlay> <o>   : "ø̛"
<dead_horn> <dead_macron> <S>               : "S̛̄"
<dead_horn> <dead_macron> <s>               : "s̛̄"
<dead_horn> <Umacron>                       : "Ư̄"
<dead_horn> <umacron>                       : "ư̄"
<dead_horn> <dead_macron> <U>               : "Ư̄"
<dead_horn> <dead_macron> <u>               : "ư̄"
<dead_horn> <Ubreve>                        : "Ư̆"
<dead_horn> <ubreve>                        : "ư̆"
<dead_horn> <dead_breve> <U>                : "Ư̆"
<dead_horn> <dead_breve> <u>                : "ư̆"

