# Latin Alphabet | Stroke Rules
# [AUTHOR]: Carlos González Sanz

# Basic precomposed letter combinations
<dead_stroke> <2>                   : "ƻ"
<dead_stroke> <UA738>               : "Ꜻ"  # Ꜹ + Stroke
<dead_stroke> <UA739>               : "ꜻ"   # ꜹ + Stroke
<dead_stroke> <multiply> <A> <V>    : "Ꜻ"  # Ꜹ + Stroke
<dead_stroke> <multiply> <a> <v>    : "ꜻ"   # ꜹ + Stroke
<dead_stroke> <B>                   : "Ƀ"
<dead_stroke> <b>                   : "ƀ"
<dead_stroke> <C>                   : "Ꞓ"
<dead_stroke> <c>                   : "ꞓ"
<dead_stroke> <D>                   : "Đ"
<dead_stroke> <d>                   : "đ"
<dead_stroke> <ETH>                 : "Ꟈ"
<dead_stroke> <eth>                 : "ꟈ"
<dead_stroke> <e>                   : "ꬳ"
<dead_stroke> <F>                   : "Ꞙ"
<dead_stroke> <f>                   : "ꞙ"
<dead_stroke> <G>                   : "Ǥ"
<dead_stroke> <g>                   : "ǥ"
<dead_stroke> <H>                   : "Ħ"
<dead_stroke> <h>                   : "ħ"
<dead_stroke> <I>                   : "Ɨ"
<dead_stroke> <i>                   : "ɨ"
<dead_stroke> <U0269>               : "ᵼ"   # ɩ (Latin Iota) + Stroke
<dead_stroke> <J>                   : "Ɉ"
<dead_stroke> <j>                   : "ɉ"
<dead_stroke> <K>                   : "Ꝁ"
<dead_stroke> <k>                   : "ꝁ"
<dead_stroke> <L>                   : "Ƚ"
<dead_stroke> <l>                   : "ƚ"
<dead_stroke> <UA7DA>               : "Ƛ"  # Ꟛ (Latin Lambda) + Stroke
<dead_stroke> <UA7DB>               : "ƛ"   # ꟛ (Latin Lambda) + Stroke
<dead_stroke> <O>                   : "Ɵ"
<dead_stroke> <o>                   : "ɵ"
<dead_stroke> <P>                   : "Ᵽ"
<dead_stroke> <p>                   : "ᵽ"
<dead_stroke> <Q>                   : "Ꝗ"
<dead_stroke> <q>                   : "ꝗ"
<dead_stroke> <R>                   : "Ɍ"
<dead_stroke> <r>                   : "ɍ"
<dead_stroke> <S>                   : "Ꟊ"
<dead_stroke> <s>                   : "ꟊ"
<dead_stroke> <T>                   : "Ŧ"
<dead_stroke> <t>                   : "ŧ"
<dead_stroke> <THORN>               : "Ꝥ"
<dead_stroke> <thorn>               : "ꝥ"
<dead_stroke> <U>                   : "Ʉ"
<dead_stroke> <u>                   : "ʉ"
<dead_stroke> <U028A>               : "ᵿ"   # ʊ (Latin Upsilon) + Stroke
<dead_stroke> <Y>                   : "Ɏ"
<dead_stroke> <y>                   : "ɏ"
<dead_stroke> <Z>                   : "Ƶ"
<dead_stroke> <z>                   : "ƶ"


