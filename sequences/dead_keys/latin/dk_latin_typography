# Dashes
<U203B> <F> <minus> : "â€’"   # Figure dash
<U203B> <f> <minus> : "â€’"   # Figure dash
<U203B> <minus> <F> : "â€’"   # Figure dash
<U203B> <minus> <f> : "â€’"   # Figure dash


# Enclosed Latin Letters
<U203B> <O> <C>                             : "Â©"   # Copyright symbol
<U203B> <o> <c>                             : "Â©"   # Copyright symbol
<U203B> <O> <U0186>                         : "ğŸ„¯"   # Copyleft symbol
<U203B> <o> <U0254>                         : "ğŸ„¯"   # Copyleft symbol
<U203B> <O> <P>                             : "â„—"   # Sound copyright symbol
<U203B> <o> <p>                             : "â„—"   # Sound copyright symbol
<U203B> <O> <U2183>                         : "ğŸ„¯"   # Copyleft symbol
<U203B> <o> <U2184>                         : "ğŸ„¯"   # Copyleft symbol
<U203B> <O> <R>                             : "Â®"   # Registered trademark symbol
<U203B> <o> <r>                             : "Â®"   # Registered trademark symbol
<U203B> <parenleft> <C> <parenright>        : "Â©"   # Copyright symbol
<U203B> <parenleft> <c> <parenright>        : "Â©"   # Copyright symbol
<U203B> <parenleft> <U0186> <parenright>    : "ğŸ„¯"   # Copyleft symbol
<U203B> <parenleft> <U0254> <parenright>    : "ğŸ„¯"   # Copyleft symbol
<U203B> <parenleft> <P> <parenright>        : "â„—"   # Sound copyright symbol
<U203B> <parenleft> <p> <parenright>        : "â„—"   # Sound copyright symbol
<U203B> <parenleft> <U2183> <parenright>    : "ğŸ„¯"   # Copyleft symbol
<U203B> <parenleft> <U2184> <parenright>    : "ğŸ„¯"   # Copyleft symbol
<U203B> <parenleft> <R> <parenright>        : "Â®"   # Registered trademark symbol
<U203B> <parenleft> <r> <parenright>        : "Â®"   # Registered trademark symbol


# Miscellaneous Punctuation
<U203B> <C> <equal> : "â¸¿"    # Capitulum
<U203B> <c> <equal> : "â¸¿"    # Capitulum
<U203B> <colon> <X> : "â€»"   # Reference mark
<U203B> <colon> <x> : "â€»"   # Reference mark
