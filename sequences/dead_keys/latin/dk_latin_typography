<U203B> <equal> <c> : "â¸¿"    # Capitulum
<U203B> <P> <space> : "â¹"    # Paragraphus mark
<U203B> <p> <space> : "â¹"    # Paragraphus mark
<U203B> <S> <M>     : "â„ "    # Service mark
<U203B> <s> <m>     : "â„ "    # Service mark
<U203B> <T> <M>     : "â„¢"    # Trademark
<U203B> <t> <m>     : "â„¢"    # Trademark
<U203B> <X> <colon> : "â€»"   # Reference mark
<U203B> <x> <colon> : "â€»"   # Reference mark


# Parentheses & Brackets
<U203B> <greater> <C>   : "â¸§"   # Right sideways U-Fence
<U203B> <greater> <c>   : "â¸§"   # Right sideways U-Fence
<U203B> <less> <C>      : "â¸¦"   # Left sideways U-Fence
