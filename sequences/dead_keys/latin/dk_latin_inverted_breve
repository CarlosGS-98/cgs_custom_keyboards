# Composed inverted breve combinations (Not directly encoded in Unicode) (Unidirectional)
<dead_invertedbreve> <Ecaron>                       : "Ě̑"
<dead_invertedbreve> <ecaron>                       : "ě̑"
<dead_invertedbreve> <dead_caron> <E>               : "Ě̑"
<dead_invertedbreve> <dead_caron> <e>               : "ě̑"
<dead_invertedbreve> <Eogonek>                      : "Ę̑"
<dead_invertedbreve> <eogonek>                      : "ę̑"
<dead_invertedbreve> <dead_ogonek> <E>              : "Ę̑"
<dead_invertedbreve> <dead_ogonek> <e>              : "ę̑"
<dead_invertedbreve> <Ebelowdot>                    : "Ẹ̑"
<dead_invertedbreve> <ebelowdot>                    : "ẹ̑"
<dead_invertedbreve> <dead_belowdot> <E>            : "Ẹ̑"
<dead_invertedbreve> <dead_belowdot> <e>            : "ẹ̑"
<dead_invertedbreve> <dead_belowring> <N>           : "N̥̑"
<dead_invertedbreve> <dead_belowring> <n>           : "n̥̑"
<dead_invertedbreve> <U01EA>                        : "Ǫ̑"    # Ǫ + Inverted Breve
<dead_invertedbreve> <U01EB>                        : "ǫ̑"    # ǫ + Inverted Breve
<dead_invertedbreve> <dead_ogonek> <O>              : "Ǫ̑"
<dead_invertedbreve> <dead_ogonek> <o>              : "ǫ̑"
<dead_invertedbreve> <Obelowdot>                    : "Ọ̑"
<dead_invertedbreve> <obelowdot>                    : "ọ̑"
<dead_invertedbreve> <dead_belowdot> <O>            : "Ọ̑"
<dead_invertedbreve> <dead_belowdot> <o>            : "ọ̑"
