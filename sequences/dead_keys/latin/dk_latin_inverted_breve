<dead_invertedbreve> <U0254>              : "ɔ̑"      # ɔ + Inverted Breve
<dead_invertedbreve> <UA7CB>              : "Ɤ̑"      # Ɤ (Ram's Horn) + Inverted Breve
<dead_invertedbreve> <U0264>              : "ɤ̑"      # ɤ (Ram's Horn) + Inverted Breve
<dead_invertedbreve> <P>                  : "P̑"
<dead_invertedbreve> <p>                  : "p̑"
<dead_invertedbreve> <Q>                  : "Q̑"
<dead_invertedbreve> <q>                  : "q̑"
