# Composed vertical line above combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main source]: https://fr.wikipedia.org/wiki/Ligne_verticale_en_chef
#
<dead_aboveverticalline> <dead_belowdiaeresis> <A>  : "A̤̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <a>  : "a̤̍"
<dead_aboveverticalline> <dead_tilde> <A>           : "Ã̍"
<dead_aboveverticalline> <dead_tilde> <a>           : "ã̍"
<dead_aboveverticalline> <dead_cedilla> <A>         : "A̧̍"
<dead_aboveverticalline> <dead_cedilla> <a>         : "a̧̍"
<dead_aboveverticalline> <dead_diaeresis> <E>       : "Ë̍"
<dead_aboveverticalline> <dead_diaeresis> <e>       : "ë̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <E>  : "E̤̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <e>  : "e̤̍"
<dead_aboveverticalline> <dead_tilde> <E>           : "Ẽ̍"
<dead_aboveverticalline> <dead_tilde> <e>           : "ẽ̍"
<dead_aboveverticalline> <dead_macron> <E>          : "Ē̍"
<dead_aboveverticalline> <dead_macron> <e>          : "ē̍"
<dead_aboveverticalline> <dead_tilde> <U0190>       : "Ɛ̃̍"   # Ɛ (Latin Epsilon) + Tilde + Vertical Line Above
<dead_aboveverticalline> <dead_tilde> <U025B>       : "ɛ̃̍"   # ɛ (Latin Epsilon) + Tilde + Vertical Line Above
<dead_aboveverticalline> <dead_belowdiaeresis> <I>  : "I̤̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <i>  : "i̤̍"
<dead_aboveverticalline> <dead_tilde> <I>           : "Ĩ̍"
<dead_aboveverticalline> <dead_tilde> <i>           : "ĩ̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <O>  : "O̤̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <o>  : "o̤̍"
<dead_aboveverticalline> <dead_tilde> <O>           : "Õ̍"
<dead_aboveverticalline> <dead_tilde> <o>           : "õ̍"
<dead_aboveverticalline> <dead_macron> <O>          : "Ō̍"
<dead_aboveverticalline> <dead_macron> <o>          : "ō̍"
<dead_aboveverticalline> <dead_tilde> <U0186>       : "Ɔ̃̍"   # Ɔ + Tilde + Vertical Line Above
<dead_aboveverticalline> <dead_tilde> <U0254>       : "ɔ̃̍"   # ɔ + Tilde + Vertical Line Above
<dead_aboveverticalline> <dead_diaeresis> <U>       : "Ü̍"
<dead_aboveverticalline> <dead_diaeresis> <u>       : "ü̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <U>  : "Ṳ̍"
<dead_aboveverticalline> <dead_belowdiaeresis> <u>  : "ṳ̍"
<dead_aboveverticalline> <dead_tilde> <U>           : "Ũ̍"
<dead_aboveverticalline> <dead_tilde> <u>           : "ũ̍"

