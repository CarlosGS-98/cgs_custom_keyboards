# Basic precomposed letter combinations
<dead_cedilla> <C>  : "√á"
<dead_cedilla> <c>  : "√ß"
<dead_cedilla> <D>  : "·∏ê"
<dead_cedilla> <d>  : "·∏ë"
<dead_cedilla> <E>  : "»®"
<dead_cedilla> <e>  : "»©"
<dead_cedilla> <G>  : "ƒ¢"
<dead_cedilla> <g>  : "ƒ£"
<dead_cedilla> <H>  : "·∏®"
<dead_cedilla> <h>  : "·∏©"
<dead_cedilla> <K>  : "ƒ∂"
<dead_cedilla> <k>  : "ƒ∑"
<dead_cedilla> <L>  : "ƒª"
<dead_cedilla> <l>  : "ƒº"
<dead_cedilla> <N>  : "≈Ö"
<dead_cedilla> <n>  : "≈Ü"
<dead_cedilla> <R>  : "≈ñ"
<dead_cedilla> <r>  : "≈ó"
<dead_cedilla> <S>  : "≈û"
<dead_cedilla> <s>  : "≈ü"
<dead_cedilla> <T>  : "≈¢"
<dead_cedilla> <t>  : "≈£"


# Other precomposed cedilla combinations (Unidirectional)
<dead_cedilla> <Cacute>         : "·∏à"
<dead_cedilla> <cacute>         : "·∏â"
<dead_cedilla> <dead_acute> <C> : "·∏à"
<dead_cedilla> <dead_acute> <c> : "·∏â"
<dead_cedilla> <U0114>          : "ƒî"   # ƒî + Cedilla
<dead_cedilla> <U0115>          : "ƒï"   # ƒï + Cedilla
<dead_cedilla> <dead_breve> <E> : "ƒî"
<dead_cedilla> <dead_breve> <e> : "ƒï"


# Special letters with descender (Preceded by <dead_cedilla> <minus>)
#
# [NOTE]: This diacritic has been placed here for convenience.
#
<dead_cedilla> <minus> <H>  : "‚±ß"
<dead_cedilla> <minus> <h>  : "‚±®"
<dead_cedilla> <minus> <K>  : "‚±©"
<dead_cedilla> <minus> <k>  : "‚±™"
<dead_cedilla> <minus> <N>  : "Íûê"
<dead_cedilla> <minus> <n>  : "Íûë"
<dead_cedilla> <minus> <Z>  : "‚±´"
<dead_cedilla> <minus> <z>  : "‚±¨"


