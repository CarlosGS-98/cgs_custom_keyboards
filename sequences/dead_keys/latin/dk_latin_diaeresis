# Composed diaeresis combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main source]: https://fr.wikipedia.org/wiki/Tréma
#
# [CUSTOM DEAD KEYS]:
# - ̯ (Combining Inverted Breve Below [U032F]) -> Represents the "Inverted Breve Below" dead key
#
<dead_diaeresis> <Aacute>                                       : "Ä́"
<dead_diaeresis> <aacute>                                       : "ä́"
<dead_diaeresis> <dead_acute> <A>                               : "Ä́"
<dead_diaeresis> <dead_acute> <a>                               : "ä́"
<dead_diaeresis> <dead_acute> <Aogonek>                         : "Ą̈́"
<dead_diaeresis> <dead_acute> <aogonek>                         : "ą̈́"
<dead_diaeresis> <dead_acute> <dead_ogonek> <A>                 : "Ą̈́"
<dead_diaeresis> <dead_acute> <dead_ogonek> <a>                 : "ą̈́"
<dead_diaeresis> <Agrave>                                       : "Ä̀"
<dead_diaeresis> <agrave>                                       : "ä̀"
<dead_diaeresis> <dead_grave> <A>                               : "Ä̀"
<dead_diaeresis> <dead_grave> <a>                               : "ä̀"
<dead_diaeresis> <dead_grave> <Aogonek>                         : "Ą̈̀"
<dead_diaeresis> <dead_grave> <aogonek>                         : "ą̈̀"
<dead_diaeresis> <dead_grave> <dead_ogonek> <A>                 : "Ą̈̀"
<dead_diaeresis> <dead_grave> <dead_ogonek> <a>                 : "ą̈̀"
<dead_diaeresis> <Acircumflex>                                  : "Ä̂"
<dead_diaeresis> <acircumflex>                                  : "ä̂"
<dead_diaeresis> <dead_circumflex> <A>                          : "Ä̂"
<dead_diaeresis> <dead_circumflex> <a>                          : "ä̂"
<dead_diaeresis> <dead_circumflex> <Aogonek>                    : "Ą̈̂"
<dead_diaeresis> <dead_circumflex> <aogonek>                    : "ą̈̂"
<dead_diaeresis> <dead_circumflex> <dead_ogonek> <A>            : "Ą̈̂"
<dead_diaeresis> <dead_circumflex> <dead_ogonek> <a>            : "ą̈̂"
<dead_diaeresis> <Atilde>                                       : "Ã̈"
<dead_diaeresis> <atilde>                                       : "ã̈"
<dead_diaeresis> <dead_tilde> <A>                               : "Ã̈"
<dead_diaeresis> <dead_tilde> <a>                               : "ã̈"
<dead_diaeresis> <dead_cedilla> <A>                             : "Ä̧"
<dead_diaeresis> <dead_cedilla> <a>                             : "ä̧"
<dead_diaeresis> <U01CD>                                        : "Ä̌"   # Ǎ + Diaeresis
<dead_diaeresis> <U01CE>                                        : "ä̌"   # ǎ + Diaeresis
