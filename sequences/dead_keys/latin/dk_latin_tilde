<dead_tilde> <U028A>            : "ʊ̃"     # ʊ (Latin Upsilon) + Tilde
<dead_tilde> <U019C>            : "Ɯ̃"     # Ɯ (Turned M) + Tilde
<dead_tilde> <U026F>            : "ɯ̃"     # ɯ (Turned M) + Tilde
<dead_tilde> <U21BA> <M>        : "Ɯ̃"     # Ɯ (Turned M) + Tilde
<dead_tilde> <U21BA> <m>        : "ɯ̃"     # ɯ (Turned M) + Tilde
<dead_tilde> <U0245>            : "Ʌ̃"     # Ʌ + Tilde
<dead_tilde> <U028C>            : "ʌ̃"     # ʌ + Tilde
<dead_tilde> <U21BA> <V>        : "Ʌ̃"     # Ʌ + Tilde
<dead_tilde> <U21BA> <v>        : "ʌ̃"     # ʌ + Tilde
<dead_tilde> <U01B2>            : "Ʋ̃"     # Ʋ + Tilde
<dead_tilde> <U028B>            : "ʋ̃"     # ʋ + Tilde
<dead_tilde> <UA768>            : "Ꝩ̃"     # Ꝩ (Vend) + Tilde
<dead_tilde> <UA769>            : "ꝩ̃"     # ꝩ (Vend) + Tilde
<dead_tilde> <UA760>            : "Ꝡ̃"     # Ꝡ + Tilde
<dead_tilde> <UA761>            : "ꝡ̃"     # ꝡ + Tilde
<dead_tilde> <U1DCF> <V> <Y>    : "Ꝡ̃"
<dead_tilde> <U1DCF> <v> <y>    : "ꝡ̃"
<dead_tilde> <W>                : "W̃"
<dead_tilde> <w>                : "w̃"
<dead_tilde> <X>                : "X̃"
<dead_tilde> <x>                : "x̃"
