# Latin Alphabet | Tilde Rules
# [AUTHOR]: Carlos González Sanz

# Basic precomposed letter combinations 
<dead_tilde> <A>    : "Ã"
<dead_tilde> <a>    : "ã"
<dead_tilde> <E>    : "Ẽ"
<dead_tilde> <e>    : "ẽ"
<dead_tilde> <I>    : "Ĩ"
<dead_tilde> <i>    : "ĩ"
<dead_tilde> <N>    : "Ñ"
<dead_tilde> <n>    : "ñ"
<dead_tilde> <O>    : "Õ"
<dead_tilde> <o>    : "õ"
<dead_tilde> <U>    : "Ũ"
<dead_tilde> <u>    : "ũ"
<dead_tilde> <V>    : "Ṽ"
<dead_tilde> <v>    : "ṽ"
<dead_tilde> <Y>    : "Ỹ"
<dead_tilde> <v>    : "ỹ"


# Precomposed Vietnamese letter combinations (Unidirectional)
<dead_tilde> <Acircumflex>           : "Ẫ"
<dead_tilde> <acircumflex>           : "ẫ"
<dead_tilde> <dead_circumflex> <A>   : "Ẫ"
<dead_tilde> <dead_circumflex> <a>   : "ẫ"
<dead_tilde> <Abreve>                : "Ẵ"
<dead_tilde> <abreve>                : "ẵ"
<dead_tilde> <dead_breve> <A>        : "Ẵ"
<dead_tilde> <dead_breve> <a>        : "ẵ"
<dead_tilde> <Ecircumflex>           : "Ễ"
<dead_tilde> <ecircumflex>           : "ễ"
<dead_tilde> <dead_circumflex> <E>   : "Ễ"
<dead_tilde> <dead_circumflex> <e>   : "ễ"
<dead_tilde> <Ocircumflex>           : "Ỗ"
<dead_tilde> <ocircumflex>           : "ỗ"
<dead_tilde> <dead_circumflex> <O>   : "Ỗ"
<dead_tilde> <dead_circumflex> <o>   : "ỗ"
<dead_tilde> <Ohorn>                 : "Ỡ"
<dead_tilde> <ohorn>                 : "ỡ"
<dead_tilde> <dead_horn> <O>         : "Ỡ"
<dead_tilde> <dead_horn> <o>         : "ỡ"
<dead_tilde> <Uhorn>                 : "Ữ"
<dead_tilde> <uhorn>                 : "ữ"
<dead_tilde> <dead_horn> <U>         : "Ữ"
<dead_tilde> <dead_horn> <u>         : "ữ"


# Other precomposed tilde combinations (Unidirectional)
<dead_tilde> <Oacute>               : "Ṍ"
<dead_tilde> <oacute>               : "ṍ"
<dead_tilde> <dead_acute> <O>       : "Ṍ"
<dead_tilde> <dead_acute> <o>       : "ṍ"
<dead_tilde> <Odiaeresis>           : "Ṏ"
<dead_tilde> <odiaeresis>           : "ṏ"
<dead_tilde> <dead_diaeresis> <O>   : "Ṏ"
<dead_tilde> <dead_diaeresis> <o>   : "ṏ"
<dead_tilde> <Omacron>              : "Ȭ"
<dead_tilde> <omacron>              : "ȭ"
<dead_tilde> <dead_macron> <O>      : "Ȭ"
<dead_tilde> <dead_macron> <o>      : "ȭ"
<dead_tilde> <Uacute>               : "Ṹ"
<dead_tilde> <uacute>               : "ṹ"
<dead_tilde> <dead_acute> <U>       : "Ṹ"
<dead_tilde> <dead_acute> <u>       : "ṹ"


# Composed base letter combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main source]: https://fr.wikipedia.org/wiki/Tilde
#
# [CUSTOM DEAD KEYS]:
# - ᷏ (Combining Zigzag Below [U1DCF]) -> Represents the "Letter Fusion" dead key
# - ↺ (Anticlockwise Open Circle Arrow [U21BA]) -> Represents the "Letter Flip" dead key
#
<dead_tilde> <U2C6F>            : "Ɐ̃"      # Ɐ + Tilde
<dead_tilde> <U0250>            : "ɐ̃"      # ɐ + Tilde
<dead_tilde> <U21BA> <A>        : "Ɐ̃"      # Ɐ + Tilde
<dead_tilde> <U21BA> <a>        : "ɐ̃"      # ɐ + Tilde
<dead_tilde> <UA732>            : "Ꜳ̃"     # Ꜳ + Tilde
<dead_tilde> <UA733>            : "ꜳ̃"      # ꜳ + Tilde
<dead_tilde> <U1DCF> <A> <A>    : "Ꜳ̃"     # Ꜳ + Tilde
<dead_tilde> <U1DCF> <a> <a>    : "ꜳ̃"      # ꜳ + Tilde
<dead_tilde> <AE>               : "Æ̃"
<dead_tilde> <ae>               : "æ̃"
<dead_tilde> <U1DCF> <A> <E>    : "Æ̃"
<dead_tilde> <U1DCF> <a> <e>    : "æ̃"
<dead_tilde> <UA734>            : "Ꜵ̃"     # Ꜵ + Tilde
<dead_tilde> <UA735>            : "ꜵ̃"      # ꜵ + Tilde
<dead_tilde> <U1DCF> <A> <O>    : "Ꜵ̃"     # Ꜵ + Tilde
<dead_tilde> <U1DCF> <a> <o>    : "ꜵ̃"      # ꜵ + Tilde
<dead_tilde> <UA736>            : "Ꜷ̃"     # Ꜷ + Tilde
<dead_tilde> <UA737>            : "ꜷ̃"      # ꜷ + Tilde
<dead_tilde> <U1DCF> <A> <U>    : "Ꜷ̃"     # Ꜷ + Tilde
<dead_tilde> <U1DCF> <a> <u>    : "ꜷ̃"      # ꜷ + Tilde
<dead_tilde> <UA738>            : "Ꜹ̃"     # Ꜹ + Tilde
<dead_tilde> <UA739>            : "ꜹ̃"      # ꜹ + Tilde
<dead_tilde> <U1DCF> <A> <V>    : "Ꜹ̃"     # Ꜹ + Tilde
<dead_tilde> <U1DCF> <a> <v>    : "ꜹ̃"      # ꜹ + Tilde
<dead_tilde> <UA73C>            : "Ꜽ̃"     # Ꜽ + Tilde
<dead_tilde> <UA73D>            : "ꜽ̃"      # ꜽ + Tilde
<dead_tilde> <U1DCF> <A> <Y>    : "Ꜽ̃"     # Ꜽ + Tilde
<dead_tilde> <U1DCF> <a> <y>    : "ꜽ̃"      # ꜽ + Tilde
<dead_tilde> <U2C6D>            : "Ɑ̃"      # Ɑ (Latin Alpha) + Tilde
<dead_tilde> <U0251>            : "ɑ̃"      # ɑ (Latin Alpha) + Tilde
<dead_tilde> <B>                : "B̃"
<dead_tilde> <b>                : "b̃"
<dead_tilde> <C>                : "C̃"
<dead_tilde> <c>                : "c̃"
<dead_tilde> <D>                : "D̃"
<dead_tilde> <d>                : "d̃"
<dead_tilde> <UA779>            : "Ꝺ̃"      # Ꝺ (Insular D) + Tilde
<dead_tilde> <UA77A>            : "ꝺ̃"      # ꝺ (Insular D) + Tilde
<dead_tilde> <U0190>            : "Ɛ̃"      # Ɛ (Latin Epsilon) + Tilde
<dead_tilde> <U025B>            : "ɛ̃"      # ɛ (Latin Epsilon) + Tilde
<dead_tilde> <SCHWA>            : "Ə̃"      # Ə (Schwa) + Tilde
<dead_tilde> <schwa>            : "ə̃"      # ə (Schwa) + Tilde
<dead_tilde> <U018E>            : "Ǝ̃̃"      # Ǝ (Turned E) + Tilde
<dead_tilde> <U01DD>            : "ǝ̃"      # ǝ (Turned E) + Tilde
<dead_tilde> <U21BA> <E>        : "Ǝ̃"      # Ǝ (Turned E) + Tilde
<dead_tilde> <U21BA> <e>        : "ǝ̃"      # ǝ (Turned E) + Tilde
<dead_tilde> <F>                : "F̃"
<dead_tilde> <f>                : "f̃"
<dead_tilde> <UA77B>            : "Ꝼ̃"      # Ꝼ (Insular F) + Tilde
<dead_tilde> <UA77C>            : "ꝼ̃"      # ꝼ (Insular F) + Tilde
<dead_tilde> <G>                : "G̃"
<dead_tilde> <g>                : "g̃"
<dead_tilde> <U0194>            : "Ɣ̃"      # Ɣ (Latin Gamma) + Tilde
<dead_tilde> <U0263>            : "ɣ̃"      # ɣ (Latin Gamma) + Tilde
<dead_tilde> <UA78D>            : "Ɥ̃"      # Ɥ (Turned H) + Tilde
<dead_tilde> <U0265>            : "ɥ̃"      # ɥ (Turned H) + Tilde
<dead_tilde> <U21BA> <H>        : "Ɥ̃"      # Ɥ (Turned H) + Tilde
<dead_tilde> <U21BA> <h>        : "ɥ̃"      # ɥ (Turned H) + Tilde
<dead_tilde> <U0196>            : "Ɩ̃"      # Ɩ (Latin Iota) + Tilde
<dead_tilde> <U0269>            : "ɩ̃"      # ɩ (Latin Iota) + Tilde
<dead_tilde> <J>                : "J̃"
<dead_tilde> <j>                : "ȷ̃"
<dead_tilde> <K>                : "K̃"
<dead_tilde> <k>                : "k̃"
<dead_tilde> <L>                : "L̃"
<dead_tilde> <l>                : "l̃"
<dead_tilde> <M>                : "M̃"
<dead_tilde> <m>                : "m̃"
<dead_tilde> <ENG>              : "Ŋ̃"      # Ŋ + Tilde
<dead_tilde> <eng>              : "ŋ̃"      # ŋ + Tilde
<dead_tilde> <OE>               : "Œ̃"
<dead_tilde> <oe>               : "œ̃"
<dead_tilde> <U1DCF> <O> <E>    : "Œ̃"
<dead_tilde> <U1DCF> <o> <e>    : "œ̃"
<dead_tilde> <UA74E>            : "Ꝏ̃"     # Ꝏ + Tilde
<dead_tilde> <UA74F>            : "ꝏ̃"     # ꝏ + Tilde
<dead_tilde> <U1DCF> <O> <O>    : "Ꝏ̃"     # Ꝏ + Tilde
<dead_tilde> <U1DCF> <o> <o>    : "ꝏ̃"     # ꝏ + Tilde
<dead_tilde> <U0186>            : "Ɔ̃"      # Ɔ + Tilde
<dead_tilde> <U0254>            : "ɔ̃"      # ɔ + Tilde
<dead_tilde> <UA7CB>            : "Ɤ̃"      # Ɤ (Ram's Horn) + Tilde
<dead_tilde> <U0264>            : "ɤ̃"      # ɤ (Ram's Horn) + Tilde
<dead_tilde> <Q>                : "Q̃"
<dead_tilde> <q>                : "q̃"
<dead_tilde> <R>                : "R̃"
