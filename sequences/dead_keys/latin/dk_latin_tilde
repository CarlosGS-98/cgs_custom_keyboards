# Composed tilde combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main source]: https://fr.wikipedia.org/wiki/Tilde
#
# [CUSTOM DEAD KEYS]:
# - ̯ (Combining Inverted Breve Below [U032F]) -> Represents the "Inverted Breve Below" dead key
#
<dead_tilde> <Aacute>                                           : "Ã́"
<dead_tilde> <aacute>                                           : "ã́"
<dead_tilde> <dead_acute> <A>                                   : "Ã́"
<dead_tilde> <dead_acute> <a>                                   : "ã́"
<dead_tilde> <dead_acute> <Amacron>                             : "Ā̃́"
<dead_tilde> <dead_acute> <amacron>                             : "ā̃́"
<dead_tilde> <dead_acute> <dead_macron> <A>                     : "Ā̃́"
<dead_tilde> <dead_acute> <dead_macron> <a>                     : "ā̃́"
<dead_tilde> <Agrave>                                           : "Ã̀"
<dead_tilde> <agrave>                                           : "ã̀"
<dead_tilde> <dead_grave> <A>                                   : "Ã̀"
<dead_tilde> <dead_grave> <a>                                   : "ã̀"
<dead_tilde> <dead_belowcircumflex> <A>                         : "Ã̭"
<dead_tilde> <dead_belowcircumflex> <a>                         : "ã̭"
<dead_tilde> <Adiaeresis>                                       : "Ã̈"
<dead_tilde> <adiaeresis>                                       : "ã̈"
<dead_tilde> <dead_diaeresis> <A>                               : "Ã̈"
<dead_tilde> <dead_diaeresis> <a>                               : "ã̈"
<dead_tilde> <Aring>                                            : "Å̃"
<dead_tilde> <aring>                                            : "å̃"
<dead_tilde> <dead_abovering> <A>                               : "Å̃"
<dead_tilde> <dead_abovering> <a>                               : "å̃"
<dead_tilde> <dead_belowring> <A>                               : "Ḁ̃"
<dead_tilde> <dead_belowring> <a>                               : "ḁ̃"
<dead_tilde> <U01CD>                                            : "Ã̌"   # Ǎ + Tilde
<dead_tilde> <U01CE>                                            : "ã̌"   # ǎ + Tilde
<dead_tilde> <dead_caron> <A>                                   : "Ã̌"
<dead_tilde> <dead_caron> <a>                                   : "ã̌"
<dead_tilde> <Aogonek>                                          : "Ą̃"
<dead_tilde> <aogonek>                                          : "ą̃"
<dead_tilde> <dead_ogonek> <A>                                  : "Ą̃"
<dead_tilde> <dead_ogonek> <a>                                  : "ą̃"
<dead_tilde> <dead_ogonek> <Amacron>                            : "Ą̄̃"
<dead_tilde> <dead_ogonek> <amacron>                            : "ą̄̃"
<dead_tilde> <dead_ogonek> <dead_macron> <A>                    : "Ą̄̃"
<dead_tilde> <dead_ogonek> <dead_macron> <a>                    : "ą̄̃"
<dead_tilde> <Amacron>                                          : "Ã̄"
<dead_tilde> <amacron>                                          : "ã̄"
<dead_tilde> <dead_macron> <A>                                  : "Ã̄"
<dead_tilde> <dead_macron> <a>                                  : "ã̄"
