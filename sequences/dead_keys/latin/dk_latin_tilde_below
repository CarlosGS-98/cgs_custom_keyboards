# Composed tilde below combinations (Not directly encoded in Unicode) (Unidirectional)
# [Main source]: https://fr.wikipedia.org/wiki/Tilde_souscrit
#
<dead_belowtilde> <Aacute>              : "Á̰"
<dead_belowtilde> <aacute>              : "á̰"
<dead_belowtilde> <dead_acute> <A>      : "Á̰"
<dead_belowtilde> <dead_acute> <a>      : "á̰"
<dead_belowtilde> <Agrave>              : "À̰"
<dead_belowtilde> <agrave>              : "à̰"
<dead_belowtilde> <dead_grave> <A>      : "À̰"
<dead_belowtilde> <dead_grave> <a>      : "à̰"
<dead_belowtilde> <Amacron>             : "Ā̰"
<dead_belowtilde> <amacron>             : "ā̰"
<dead_belowtilde> <dead_macron> <A>     : "Ā̰"
<dead_belowtilde> <dead_macron> <a>     : "ā̰"
<dead_belowtilde> <Eacute>              : "Ḛ́"
<dead_belowtilde> <eacute>              : "ḛ́"
<dead_belowtilde> <dead_acute> <E>      : "Ḛ́"
<dead_belowtilde> <dead_acute> <e>      : "ḛ́"
<dead_belowtilde> <Egrave>              : "Ḛ̀"
<dead_belowtilde> <egrave>              : "ḛ̀"
<dead_belowtilde> <dead_grave> <E>      : "Ḛ̀"
<dead_belowtilde> <dead_grave> <e>      : "ḛ̀"
<dead_belowtilde> <Emacron>             : "Ḛ̄"
<dead_belowtilde> <emacron>             : "ḛ̄"
<dead_belowtilde> <dead_macron> <E>     : "Ḛ̄"
<dead_belowtilde> <dead_macron> <e>     : "ḛ̄"
<dead_belowtilde> <dead_acute> <U0190>  : "Ɛ̰́"   # Ɛ (Latin Epsilon) + Acute + Tilde Below
<dead_belowtilde> <dead_acute> <U025B>  : "ɛ̰́"   # ɛ (Latin Epsilon) + Acute + Tilde Below
<dead_belowtilde> <dead_grave> <U0190>  : "Ɛ̰̀"   # Ɛ (Latin Epsilon) + Grave + Tilde Below
<dead_belowtilde> <dead_grave> <U0190>  : "ɛ̰̀"   # ɛ (Latin Epsilon) + Grave + Tilde Below
<dead_belowtilde> <dead_macron> <U0190> : "Ɛ̰̄"   # Ɛ (Latin Epsilon) + Tilde Below + Macron
<dead_belowtilde> <dead_macron> <U025B> : "ɛ̰̄"   # ɛ (Latin Epsilon) + Tilde Below + Macron
<dead_belowtilde> <dead_acute> <SCHWA>  : "Ə̰́"
<dead_belowtilde> <dead_acute> <schwa>  : "ə̰́"
<dead_belowtilde> <dead_grave> <SCHWA>  : "Ə̰̀"
<dead_belowtilde> <dead_grave> <schwa>  : "ə̰̀"
<dead_belowtilde> <dead_macron> <SCHWA> : "Ə̰̄"
<dead_belowtilde> <dead_macron> <schwa> : "ə̰̄"
<dead_belowtilde> <Iacute>              : "Ḭ́"
<dead_belowtilde> <iacute>              : "Ḭ́"
<dead_belowtilde> <dead_acute> <I>      : "ḭ́"
<dead_belowtilde> <dead_acute> <i>      : "ḭ́"
