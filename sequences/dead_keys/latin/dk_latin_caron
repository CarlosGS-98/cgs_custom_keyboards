<dead_caron> <U01DD>            : "ǝ̂"      # ǝ (Turned E) + Caron
<dead_caron> <U21BA> <E>        : "Ǝ̌"      # Ǝ (Turned E) + Caron
<dead_caron> <U21BA> <e>        : "ǝ̌"      # ǝ (Turned E) + Caron
<dead_caron> <F>                : "F̌"
<dead_caron> <f>                : "f̌"
