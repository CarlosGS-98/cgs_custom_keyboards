<dead_caron> <U0264>            : "ɤ̂"      # ɤ (Ram's Horn) + Caron
<dead_caron> <P>                : "P̌"
<dead_caron> <p>                : "p̌"
<dead_caron> <Q>                : "Q̌"
<dead_caron> <q>                : "q̌"
