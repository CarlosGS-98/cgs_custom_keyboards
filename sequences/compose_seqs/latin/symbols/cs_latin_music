<Multi_key> <numbersign> <p> <e> <d> <space>    : "𝆮"   # Pedal Mark


# Modern Musical Notation (Scores) (Alphabetical)
<Multi_key> <numbersign> <C> <space>            : "𝄴"   # Common Time
<Multi_key> <numbersign> <c> <space>            : "𝄴"   # Common Time
