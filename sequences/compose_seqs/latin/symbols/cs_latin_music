<Multi_key> <numbersign> <p> <space>    : "𝆏"   # Piano
<Multi_key> <numbersign> <R> <space>    : "𝆌"   # Rinforzando
<Multi_key> <numbersign> <r> <space>    : "𝆌"   # Rinforzando
<Multi_key> <numbersign> <S> <space>    : "𝆍"   # Subito
<Multi_key> <numbersign> <s> <space>    : "𝆍"   # Subito
<Multi_key> <numbersign> <Z> <space>    : "𝆎"   # Musical Symbol Z
<Multi_key> <numbersign> <z> <space>    : "𝆎"   # Musical Symbol Z


