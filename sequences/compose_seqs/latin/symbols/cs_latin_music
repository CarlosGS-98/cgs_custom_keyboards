<Multi_key> <numbersign> <c> <bar> <space>      : "𝄵"   # Cut Time
<Multi_key> <numbersign> <C> <slash> <space>    : "𝄵"   # Cut Time
<Multi_key> <numbersign> <c> <slash> <space>    : "𝄵"   # Cut Time
<Multi_key> <numbersign> <D> <C> <space>        : "𝄊"   # Da Capo
<Multi_key> <numbersign> <d> <c> <space>        : "𝄊"   # Da Capo
<Multi_key> <numbersign> <D> <S> <space>        : "𝄉"   # Dal Segno
<Multi_key> <numbersign> <d> <s> <space>        : "𝄉"   # Dal Segno
<Multi_key> <numbersign> <E> <minus> <space>    : "𝆨"   # Endstimme
<Multi_key> <numbersign> <e> <minus> <space>    : "𝆨"   # Endstimme
<Multi_key> <numbersign> <H> <minus> <space>    : "𝆦"   # Hauptstimme
<Multi_key> <numbersign> <h> <minus> <space>    : "𝆦"   # Hauptstimme
<Multi_key> <numbersign> <N> <minus> <space>    : "𝆧"   # Nebenstimme
<Multi_key> <numbersign> <n> <minus> <space>    : "𝆧"   # Nebenstimme


# Modern Musical Notation (Tones) (Alphabetical)
<Multi_key> <numbersign> <8> <V> <A> <space>        : "𝄶"   # Ottava Alta
<Multi_key> <numbersign> <8> <v> <a> <space>        : "𝄶"   # Ottava Alta
<Multi_key> <numbersign> <8> <V> <B> <space>        : "𝄷"   # Ottava Bassa
<Multi_key> <numbersign> <8> <v> <b> <space>        : "𝄷"   # Ottava Bassa
<Multi_key> <numbersign> <1> <5> <M> <A> <space>    : "𝄸"   # Quindicesima Alta
<Multi_key> <numbersign> <1> <5> <m> <a> <space>    : "𝄸"   # Quindicesima Alta
<Multi_key> <numbersign> <1> <5> <M> <B> <space>    : "𝄹"   # Quindicesima Bassa
<Multi_key> <numbersign> <1> <5> <m> <b> <space>    : "𝄹"   # Quindicesima Bassa
<Multi_key> <numbersign> <T> <R> <space>            : "𝆖"    # Musical Trill
<Multi_key> <numbersign> <t> <r> <space>            : "𝆖"    # Musical Trill

