# Latin Alphabet | SMK Superscript Rules
# [AUTHOR]: Carlos Gonz√°lez Sanz
#
# > The purpose of this file is to store additional
# > Compose rules for typing superscript and overscript characters
# > using the circumflex accent (<dead_circumflex>)
# > in conjunction with the Compose Key (<Multi_key>).
#
# [Main source]: https://en.wikipedia.org/wiki/Unicode_subscripts_and_superscripts
#

# Skorpian Overscript Circumflex (Greek Letters)
<Multi_key> <dead_circumflex> <dead_circumflex> <dead_greek> <b>    : "·∑©"


# Skorpian Overscript Circumflex (Latin Letters)
<Multi_key> <dead_circumflex> <dead_circumflex> <a>                                 : "Õ£"
<Multi_key> <dead_circumflex> <dead_circumflex> <adiaeresis>                        : "·∑≤"
<Multi_key> <dead_circumflex> <dead_circumflex> <dead_diaeresis> <a>                : "·∑≤"
<Multi_key> <dead_circumflex> <dead_circumflex> <ae>                                : "·∑î"
<Multi_key> <dead_circumflex> <dead_circumflex> <U1DCF> <a> <e>                     : "·∑î"
<Multi_key> <dead_circumflex> <dead_circumflex> <UA735>                             : "·∑ï"    # Íúµ + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <U1DCF> <a> <o>                     : "·∑ï"
<Multi_key> <dead_circumflex> <dead_circumflex> <UA739>                             : "·∑ñ"    # Íúπ + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <U1DCF> <a> <v>                     : "·∑ñ"
<Multi_key> <dead_circumflex> <dead_circumflex> <U0251>                             : "·∑ß"    # …ë (Latin Alpha) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <dead_greek> <dead_circumflex> <a>  : "·∑ß"    # …ë (Latin Alpha) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <b>                                 : "·∑®"
<Multi_key> <dead_circumflex> <dead_circumflex> <c>                                 : "Õ®"
<Multi_key> <dead_circumflex> <dead_circumflex> <ccedilla>                          : "·∑ó"
<Multi_key> <dead_circumflex> <dead_circumflex> <dead_cedilla> <c>                  : "·∑ó"
<Multi_key> <dead_circumflex> <dead_circumflex> <d>                                 : "Õ©"
<Multi_key> <dead_circumflex> <dead_circumflex> <eth>                               : "·∑ô"
<Multi_key> <dead_circumflex> <dead_circumflex> <A77A>                              : "·∑ò"    # Íù∫ (Insular D) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <e>                                 : "Õ§"
<Multi_key> <dead_circumflex> <dead_circumflex> <schwa>                             : "·∑™"
<Multi_key> <dead_circumflex> <dead_circumflex> <U035A> <e>                         : "·∑™"
<Multi_key> <dead_circumflex> <dead_circumflex> <U01DD>                             : "·∑™"    # ∆é (Turned E) + Superscript
<Multi_key> <dead_circumflex> <dead_circumflex> <U21BA> <e>                         : "·∑™"
<Multi_key> <dead_circumflex> <dead_circumflex> <f>                                 : "·∑´"
<Multi_key> <dead_circumflex> <dead_circumflex> <minus> <G>                         : "·∑õ"    # Smallcaps G
<Multi_key> <dead_circumflex> <dead_circumflex> <g>                                 : "·∑ö"
<Multi_key> <dead_circumflex> <dead_circumflex> <U1D79>                             : "·´å"    # ·µπ (Insular G) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <h>                                 : "Õ™"
<Multi_key> <dead_circumflex> <dead_circumflex> <i>                                 : "Õ•"
<Multi_key> <dead_circumflex> <dead_circumflex> <k>                                 : "·∑ú"
<Multi_key> <dead_circumflex> <dead_circumflex> <minus> <L>                         : "·∑û"    # Smallcaps L
<Multi_key> <dead_circumflex> <dead_circumflex> <l>                                 : "·∑ù"
<Multi_key> <dead_circumflex> <dead_circumflex> <U0334> <l>                         : "·∑¨"    # …´ + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <minus> <M>                         : "·∑ü"
<Multi_key> <dead_circumflex> <dead_circumflex> <m>                                 : "Õ´"
<Multi_key> <dead_circumflex> <dead_circumflex> <minus> <N>                         : "·∑°"    # Smallcaps N
<Multi_key> <dead_circumflex> <dead_circumflex> <n>                                 : "·∑†"
<Multi_key> <dead_circumflex> <dead_circumflex> <O>                                 : "·∑≠"
<Multi_key> <dead_circumflex> <dead_circumflex> <o>                                 : "Õ¶"
<Multi_key> <dead_circumflex> <dead_circumflex> <odiaeresis>                        : "·∑≥"
<Multi_key> <dead_circumflex> <dead_circumflex> <dead_diaeresis> <o>                : "·∑≥"
<Multi_key> <dead_circumflex> <dead_circumflex> <p>                                 : "·∑Æ"
<Multi_key> <dead_circumflex> <dead_circumflex> <minus> <R>                         : "·∑¢"    # Smallcaps R
<Multi_key> <dead_circumflex> <dead_circumflex> <r>                                 : "Õ¨"
<Multi_key> <dead_circumflex> <dead_circumflex> <UA783>                             : "·´ç"    # ÍûÉ (Insular R) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <UA75B>                             : "·∑£"    # Íùõ (R Rotunda) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <s>                                 : "·∑§"
<Multi_key> <dead_circumflex> <dead_circumflex> <U017F>                             : "·∑•"    # ≈ø (Long S) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <U0283>                             : "·∑Ø"    #  É (Esh) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <U035A> <s>                         : "·∑Ø"
<Multi_key> <dead_circumflex> <dead_circumflex> <t>                                 : "Õ≠"
<Multi_key> <dead_circumflex> <dead_circumflex> <UA787>                             : "·´é"    # Íûá (Insular T) + Overscript
<Multi_key> <dead_circumflex> <dead_circumflex> <U>                                 : "·∑∞"
<Multi_key> <dead_circumflex> <dead_circumflex> <u>                                 : "Õß"
<Multi_key> <dead_circumflex> <dead_circumflex> <udiaeresis>                        : "·∑¥"
<Multi_key> <dead_circumflex> <dead_circumflex> <dead_diaeresis> <u>                : "·∑¥"
<Multi_key> <dead_circumflex> <dead_circumflex> <v>                                 : "ÕÆ"
<Multi_key> <dead_circumflex> <dead_circumflex> <w>                                 : "·∑±"
<Multi_key> <dead_circumflex> <dead_circumflex> <x>                                 : "ÕØ"
<Multi_key> <dead_circumflex> <dead_circumflex> <z>                                 : "·∑¶"


# Skorpian Superscript Circumflex (Greek Letters)
<Multi_key> <dead_circumflex> <dead_greek> <b>              : "·µù"
<Multi_key> <dead_circumflex> <dead_greek> <g>              : "·µû"
<Multi_key> <dead_circumflex> <dead_greek> <d>              : "·µü"
<Multi_key> <dead_circumflex> <dead_greek> <e>              : "·µã"
<Multi_key> <dead_circumflex> <dead_greek> <thorn>          : "·∂ø"
<Multi_key> <dead_circumflex> <dead_greek> <minus> <t> <h>  : "·∂ø"
<Multi_key> <dead_circumflex> <dead_greek> <i>              : "·∂•"
<Multi_key> <dead_circumflex> <dead_greek> <f>              : "·µ†"
<Multi_key> <dead_circumflex> <dead_greek> <j>              : "·µ°"


# Skorpian Superscript Circumflex (Latin Letters)
<Multi_key> <dead_circumflex> <A>                                           : "·¥¨"
<Multi_key> <dead_circumflex> <a>                                           : "·µÉ"
<Multi_key> <dead_circumflex> <AE>                                          : "·¥≠"
