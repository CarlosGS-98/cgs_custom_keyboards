<Multi_key> <dead_circumflex> <ezh>                                         : "·∂æ"
<Multi_key> <dead_circumflex> <U0294>                                       : "ÀÄ"   #  î (Glottal Stop) + Superscript
<Multi_key> <dead_circumflex> <U035A> <U0241> <space>                       : "ÀÄ"   # …Å (Capital Glottal Stop) + IPA Key
<Multi_key> <dead_circumflex> <U035A> <U0242> <space>                       : "ÀÄ"   # …Ç (Small Glottal Stop) + IPA Key
<Multi_key> <dead_circumflex> <U035A> <U0241> <minus>                       : "êû≥"   # …Å (Capital Glottal Stop) + "-" + IPA Key + Superscript
<Multi_key> <dead_circumflex> <U035A> <U0242> <minus>                       : "êû≥"   # …Ç (Small Glottal Stop) + "-" + IPA Key + Superscript
<Multi_key> <dead_circumflex> <dead_stroke> <U0294>                         : "êû≥"   #  î (Glottal Stop) + Stroke + Superscript
<Multi_key> <dead_circumflex> <dead_stroke> <U035A> <question>              : "êû≥"   #  î (Glottal Stop) + Stroke + Superscript
<Multi_key> <dead_circumflex> <dead_stroke> <U035A> <U0241> <space>         : "êû≥"   # …Å (Capital Glottal Stop) + Stroke + Superscript
<Multi_key> <dead_circumflex> <dead_stroke> <U035A> <U0242> <space>         : "êû≥"   # …Ç (Small Glottal Stop) + Stroke + Superscript
<Multi_key> <dead_circumflex> <U0295>                                       : "À§"   #  ï (Reversed Glottal Stop) + Superscript
<Multi_key> <dead_circumflex> <U035A> <U0241> <underscore> <space>          : "ÀÅ"   # …Å (Capital Glottal Stop) + "_" + IPA Key
<Multi_key> <dead_circumflex> <U035A> <U0242> <underscore> <space>          : "À§"   # …Ç (Small Glottal Stop) + "_" + IPA Key

