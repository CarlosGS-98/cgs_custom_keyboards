# Modern Musical Notation (Combining Tremoli) (Non-Alphabetical)
<Multi_key> <numbersign> <dead_circumflex> <dead_tilde> <1> <space> : "𝅪"   # Combining Single Fingered Tremolo
<Multi_key> <numbersign> <dead_circumflex> <dead_tilde> <2> <space> : "𝅫"   # Combining Double Fingered Tremolo
<Multi_key> <numbersign> <dead_circumflex> <dead_tilde> <3> <space> : "𝅬"   # Combining Triple Fingered Tremolo
<Multi_key> <numbersign> <dead_circumflex> <slash> <1> <space>      : "𝅧"   # Combining Single Tremolo
<Multi_key> <numbersign> <dead_circumflex> <slash> <2> <space>      : "𝅨"   # Combining Double Tremolo
<Multi_key> <numbersign> <dead_circumflex> <slash> <3> <space>      : "𝅩"   # Combining Triple Tremolo


# Modern Musical Notation (Flourishes & Ornaments) (Non-Alphabetical)
<Multi_key> <numbersign> <dollar> <Left> <space>            : "𝆘"   # Musical Inverted Turn
<Multi_key> <numbersign> <dollar> <Right> <space>           : "𝆗"   # Musical Turn
<Multi_key> <numbersign> <dollar> <slash> <Right> <space>   : "𝆙"   # Musical Slashed Turn
<Multi_key> <numbersign> <dollar> <Up> <space>              : "𝆚"   # Musical Turn Up
<Multi_key> <numbersign> <masculine> <1> <space>            : "𝆛"   # Ornament Stroke-1
<Multi_key> <numbersign> <masculine> <2> <space>            : "𝆜"   # Ornament Stroke-2
<Multi_key> <numbersign> <masculine> <3> <space>            : "𝆝"   # Ornament Stroke-3
<Multi_key> <numbersign> <masculine> <4> <space>            : "𝆞"   # Ornament Stroke-4
<Multi_key> <numbersign> <masculine> <5> <space>            : "𝆟"   # Ornament Stroke-5
<Multi_key> <numbersign> <masculine> <6> <space>            : "𝆠"   # Ornament Stroke-6
<Multi_key> <numbersign> <masculine> <7> <space>            : "𝆡"   # Ornament Stroke-7
<Multi_key> <numbersign> <masculine> <8> <space>            : "𝆢"   # Ornament Stroke-8
<Multi_key> <numbersign> <masculine> <9> <space>            : "𝆣"   # Ornament Stroke-9
<Multi_key> <numbersign> <masculine> <1> <0> <space>        : "𝆤"   # Ornament Stroke-10
<Multi_key> <numbersign> <masculine> <1> <1> <space>        : "𝆥"   # Ornament Stroke-11


