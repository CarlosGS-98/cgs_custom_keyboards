<Multi_key> <numbersign> <greater> <greater> <space>    : "𝆓"   # Decrescendo
<Multi_key> <numbersign> <less> <less> <space>          : "𝆒"   # Crescendo
<Multi_key> <numbersign> <slash> <0> <space>            : "𝆩"   # Musical Degree Slash/Niente


# Modern Musical Notation (Note Breaks/Rests) (Non-Alphabetical)
<Multi_key> <numbersign> <apostrophe> <space>                   : "𝄒"    # Breath Mark/Luftpause
<Multi_key> <numbersign> <at> <1> <space>                       : "𝄻"    # Whole Note Rest
<Multi_key> <numbersign> <at> <1> <slash> <1> <space>           : "𝄻"    # Whole Note Rest
<Multi_key> <numbersign> <at> <1> <slash> <2> <space>           : "𝄼"    # Half Note Rest
<Multi_key> <numbersign> <at> <1> <slash> <4> <space>           : "𝄽"    # Quarter Note Rest
<Multi_key> <numbersign> <at> <1> <slash> <8> <space>           : "𝄾"    # Eight Note Rest
<Multi_key> <numbersign> <at> <1> <slash> <1> <6> <space>       : "𝄿"    # Sixteenth Note Rest
<Multi_key> <numbersign> <at> <1> <slash> <3> <2> <space>       : "𝅀"    # Thirty-Second Rest
<Multi_key> <numbersign> <at> <1> <slash> <6> <4> <space>       : "𝅁"    # Sixty-Fourth Rest
<Multi_key> <numbersign> <at> <1> <slash> <1> <2> <8> <space>   : "𝅂"    # One Hundred Twenty-Eighth Rest
<Multi_key> <numbersign> <bracketright> <bracketleft> <space>   : "𝄺"   # Multiple Rest
<Multi_key> <numbersign> <minus> <space>                        : "𝄩"   # Multiple Measure Rest
<Multi_key> <numbersign> <quotedbl> <space>                     : "𝄓"   # Cæsura


# Modern Musical Notation (Noteheads) (Non-Alphabetical)
<Multi_key> <numbersign> <plus> <asciicircum> <underscore> <space>                              : "𝅈"   # White Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <underscore> <Down> <space>                       : "𝅎"   # White Down Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <underscore> <Left> <space>                       : "𝅊"   # White Left Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <underscore> <Right> <space>                      : "𝅌"   # White Right Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <underscore> <Up> <space>                         : "𝅐"   # White Up Right Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <asterisk> <underscore> <space>                   : "𝅉"   # Black Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <asterisk> <underscore> <Down> <space>            : "𝅏"   # Black Down Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <asterisk> <underscore> <Left> <space>            : "𝅋"   # Black Left Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <asterisk> <underscore> <Right> <space>           : "𝅍"   # Black Right Triangle Notehead
<Multi_key> <numbersign> <plus> <asciicircum> <asterisk> <underscore> <Up> <space>              : "𝅑"   # Black Up Right Triangle Notehead
<Multi_key> <numbersign> <plus> <asterisk> <underscore> <space>                                 : "𝅘"   # Black Notehead
<Multi_key> <numbersign> <plus> <bar> <degree> <underscore> <space>                             : "𝅒"   # White Moon Notehead
<Multi_key> <numbersign> <plus> <bar> <asterisk> <degree> <underscore> <space>                  : "𝅓"   # Black Moon Notehead
<Multi_key> <numbersign> <plus> <bracketleft> <bracketright> <space>                            : "𝅚"   # White Cluster Notehead
<Multi_key> <numbersign> <plus> <bracketleft> <bracketright> <asterisk> <space>                 : "𝅛"   # Black Cluster Notehead
<Multi_key> <numbersign> <plus> <bracketleft> <bracketright> <underscore> <space>               : "𝅆"   # White Square Notehead
<Multi_key> <numbersign> <plus> <bracketleft> <asterisk> <bracketright> <underscore> <space>    : "𝅇"   # Black Square Notehead
<Multi_key> <numbersign> <plus> <minus> <degree> <underscore> <space>                           : "𝅔"   # White Down Round Triangle Notehead
<Multi_key> <numbersign> <plus> <minus> <asterisk> <degree> <underscore> <space>                : "𝅕"   # Black Down Round Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <underscore> <space>                          : "𝅈"   # White Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <underscore> <Down> <space>                   : "𝅎"   # White Down Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <underscore> <Left> <space>                   : "𝅊"   # White Left Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <underscore> <Right> <space>                  : "𝅌"   # White Right Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <underscore> <Up> <space>                     : "𝅐"   # White Up Right Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <asterisk> <underscore> <space>               : "𝅉"   # Black Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <asterisk> <underscore> <Down> <space>        : "𝅏"   # Black Down Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <asterisk> <underscore> <Left> <space>        : "𝅋"   # Black Left Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <asterisk> <underscore> <Right> <space>       : "𝅍"   # Black Right Triangle Notehead
<Multi_key> <numbersign> <plus> <dead_circumflex> <asterisk> <underscore> <Up> <space>          : "𝅑"   # Black Up Right Triangle Notehead
<Multi_key> <numbersign> <plus> <degree> <underscore> <space>                                   : "𝅅"   # Circle-X Notehead
<Multi_key> <numbersign> <plus> <parenleft> <parenright> <underscore> <space>                   : "𝅖"   # Parenthesis Notehead
<Multi_key> <numbersign> <plus> <underscore> <space>                                            : "𝅄"   # Plus Notehead
<Multi_key> <numbersign> <plus> <underscore> <3> <space>                                        : "𝅙"   # Null Notehead
<Multi_key> <numbersign> <plus> <underscore> <underscore> <space>                               : "𝅗"   # Void Notehead


# Modern Musical Notation (Note Length & Symbols) (Non-Alphabetical)
<Multi_key> <numbersign> <8> <Up> <space>                                           : "𝆕"   # Grace Note
<Multi_key> <numbersign> <2> <slash> <1> <space>                                    : "𝅜"   # Breve/Double Whole Note            
<Multi_key> <numbersign> <1> <slash> <1> <space>                                    : "𝅝"   # Whole Note
<Multi_key> <numbersign> <1> <slash> <2> <space>                                    : "𝅗𝅥"   # Half Note
<Multi_key> <numbersign> <1> <slash> <4> <space>                                    : "𝅘𝅥"   # Quarter Note
<Multi_key> <numbersign> <1> <slash> <8> <space>                                    : "𝅘𝅥𝅮"   # Eighth Note
<Multi_key> <numbersign> <1> <slash> <1> <6> <space>                                : "𝅘𝅥𝅯"   # Sixteenth Note
<Multi_key> <numbersign> <1> <slash> <3> <2> <space>                                : "𝅘𝅥𝅰"   # Thirty-Second Note
<Multi_key> <numbersign> <1> <slash> <6> <4> <space>                                : "𝅘𝅥𝅱"   # Sixty-Fourth Note
<Multi_key> <numbersign> <1> <slash> <1> <2> <8> <space>                            : "𝅘𝅥𝅰"   # Hundred-Twenty-Eighth Note
<Multi_key> <numbersign> <slash> <8> <Up> <space>                                   : "𝆔"   # Slashed Grace Note
<Multi_key> <numbersign> <underscore> <1> <slash> <4> <space>                       : "♩"   # Quarter Note (BMP)
<Multi_key> <numbersign> <underscore> <1> <slash> <8> <space>                       : "♪"   # Eighth Note (BMP)
<Multi_key> <numbersign> <underscore> <1> <slash> <8> <asterisk> <2> <space>        : "♫"   # Beamed Eight Notes (BMP)
<Multi_key> <numbersign> <underscore> <1> <slash> <1> <6> <asterisk> <2> <space>    : "♬"   # Beamed Sixteenth Notes (BMP)
<Multi_key> <numbersign> <asciicircum> <period> <space>                             : "𝄐"   # Fermata Above
<Multi_key> <numbersign> <dead_circumflex> <period> <space>                         : "𝄐"   # Fermata Above
<Multi_key> <numbersign> <underscore> <period> <space>                              : "𝄑"   # Fermata Below


